<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [ 
   <!ENTITY repr "http://sweet.jpl.nasa.gov/2.3/repr.owl#">    
   <!ENTITY math "http://sweet.jpl.nasa.gov/2.3/reprMath.owl">    
   <!ENTITY mrela "http://sweet.jpl.nasa.gov/2.3/relaMath.owl">    
   <!ENTITY owl "http://www.w3.org/2002/07/owl#">    
   <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#"> 
   <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">    
   <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#"> 
]>

<rdf:RDF 
   xml:base = "&repr;"
   xmlns:repr = "&repr;"
   xmlns:math = "&math;"
   xmlns:mrela= "&mrela;"
   xmlns:owl = "&owl;"
   xmlns:rdf = "&rdf;"
   xmlns:rdfs = "&rdfs;"
   xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&math;"/>
     <owl:imports rdf:resource="&mrela;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#repres"/>

  <owl:Class rdf:about="#knowledg">
    <rdfs:subClassOf rdf:resource="#repres"/>
  </owl:Class>

  <owl:Class rdf:about="#measur">
    <rdfs:subClassOf rdf:resource="#repres"/>
  </owl:Class>

<!-- Measurement Scales -->
  <owl:Class rdf:about="#measurementsc">
    <owl:equivalentClass rdf:resource="#scale"/>
  </owl:Class>

  <owl:Class rdf:about="#intervalsc">
    <rdfs:subClassOf rdf:resource="#quantitativesc"/>
  </owl:Class>

  <owl:Class rdf:about="#categoricalsc">
    <rdfs:subClassOf rdf:resource="#measurementsc"/>
    <owl:disjointWith rdf:resource="#ordinalsc"/>
    <owl:disjointWith rdf:resource="#quantitativesc"/>
  </owl:Class>

  <owl:Class rdf:about="#ordinalsc">
    <rdfs:subClassOf rdf:resource="#measurementsc"/>
    <owl:disjointWith rdf:resource="#quantitativesc"/>
  </owl:Class>

  <owl:Class rdf:about="#quantitativesc">
    <rdfs:subClassOf rdf:resource="#measurementsc"/>
  </owl:Class>

  <owl:Class rdf:about="#cardinalsc">
    <rdfs:subClassOf rdf:resource="#quantitativesc"/>
  </owl:Class>

  <owl:Class rdf:about="#logarithmicsc">
    <rdfs:subClassOf rdf:resource="#quantitativesc"/>
  </owl:Class>

  <owl:Class rdf:about="#qualit">
    <rdfs:subClassOf rdf:resource="#measurementsc"/>
  </owl:Class>

  <owl:Class rdf:about="#ratiosc">
    <rdfs:subClassOf rdf:resource="#measurementsc"/>
  </owl:Class>

  <owl:Class rdf:about="#scale">
    <rdfs:subClassOf rdf:resource="#repres"/>
  </owl:Class>

<!-- Coordinates -->
  <owl:Class rdf:about="#dimen">
    <rdfs:subClassOf rdf:resource="#repres"/>
    <rdfs:comment>In common usage, a dimension is a parameter or measurement used to describe some relevant characteristic of an object. The most commonly used dimensions are the parameters describing the size of an object: length, width, and height, but dimensions can also be other physical parameters such as the mass and electric charge of an object, or even, in a context where cost is relevant, an economic parameter such as its price.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#1d">
    <rdfs:subClassOf rdf:resource="#repres"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="&xsd;integer">1</owl:hasValue>
        <owl:onProperty rdf:resource="&mrela;#hasdimen"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#2d">
    <rdfs:subClassOf rdf:resource="#repres"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="&xsd;integer">2</owl:hasValue>
        <owl:onProperty rdf:resource="&mrela;#hasdimen"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#3d">
    <rdfs:subClassOf rdf:resource="#repres"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="&xsd;integer">3</owl:hasValue>
        <owl:onProperty rdf:resource="&mrela;#hasdimen"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#coordin">
    <rdfs:subClassOf rdf:resource="&math;#array"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#coordin"/>
        <owl:onProperty rdf:resource="&mrela;#eachelementhastyp"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#coordin">
    <rdfs:subClassOf rdf:resource="&math;#scalar"/>
    <owl:equivalentClass rdf:resource="#compon"/>
  </owl:Class>
  <owl:Class rdf:about="#compon"/>

  <owl:Class rdf:about="#coordinatesystem">
    <rdfs:subClassOf rdf:resource="#referencefram"/>
  </owl:Class>

  <owl:Class rdf:about="#referencefram">
    <rdfs:subClassOf rdf:resource="&math;#numericalent"/>
  </owl:Class>
  
 <owl:Class rdf:about="#dependentvari">
    <rdfs:subClassOf rdf:resource="&math;#variabl" />
    <rdfs:comment xml:lang="en">Any variable considered as a function of other variables, the latter being called independent.</rdfs:comment>
  </owl:Class>

 <owl:Class rdf:about="#independentvari">
    <rdfs:subClassOf rdf:resource="&math;#variabl" />
  </owl:Class>

  <repr:ReferenceFrame rdf:about="#barycentr">
  </repr:ReferenceFrame>

  <repr:ReferenceFrame rdf:about="#eulerian">
  </repr:ReferenceFrame>

  <repr:ReferenceFrame rdf:about="#geocentr">
  </repr:ReferenceFrame>

  <repr:ReferenceFrame rdf:about="#geodet">
  </repr:ReferenceFrame>

  <repr:ReferenceFrame rdf:about="#heliocentr">
  </repr:ReferenceFrame>

  <repr:ReferenceFrame rdf:about="#lagrangian">
    <owl:differentFrom rdf:resource="#eulerian"/>
  </repr:ReferenceFrame>

  <repr:ReferenceFrame rdf:about="#spectral">
  </repr:ReferenceFrame>

  <repr:ReferenceFrame rdf:about="#synopt">
  </repr:ReferenceFrame>

</rdf:RDF>
