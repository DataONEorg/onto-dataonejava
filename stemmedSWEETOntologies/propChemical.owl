<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY cstate "http://sweet.jpl.nasa.gov/2.3/stateChemical.owl">
  <!ENTITY pstate "http://sweet.jpl.nasa.gov/2.3/statePhysical.owl">
  <!ENTITY chem "http://sweet.jpl.nasa.gov/2.3/matr.owl">
  <!ENTITY comp "http://sweet.jpl.nasa.gov/2.3/matrCompound.owl">
  <!ENTITY repr "http://sweet.jpl.nasa.gov/2.3/repr.owl">
  <!ENTITY indx "http://sweet.jpl.nasa.gov/2.3/propIndex.owl">
  <!ENTITY ion "http://sweet.jpl.nasa.gov/2.3/matrIon.owl">
  <!ENTITY elem "http://sweet.jpl.nasa.gov/2.3/matrElement.owl">
  <!ENTITY conc "http://sweet.jpl.nasa.gov/2.3/propMass.owl">
  <!ENTITY prop "http://sweet.jpl.nasa.gov/2.3/prop.owl">
  <!ENTITY quan "http://sweet.jpl.nasa.gov/2.3/propQuantity.owl">
  <!ENTITY units "http://sweet.jpl.nasa.gov/2.3/reprSciUnits.owl">
  <!ENTITY screla "http://sweet.jpl.nasa.gov/2.3/relaSci.owl">
  <!ENTITY crela "http://sweet.jpl.nasa.gov/2.3/relaChemical.owl">
  <!ENTITY rela "http://sweet.jpl.nasa.gov/2.3/rela.owl">
  <!ENTITY chemp "http://sweet.jpl.nasa.gov/2.3/propChemical.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&chemp;"
         xmlns:owl="&owl;"
         xmlns:cstate="&cstate;"
         xmlns:pstate="&pstate;"
         xmlns:screla="&screla;"
         xmlns:crela="&crela;"
         xmlns:rela="&rela;"
         xmlns:ion="&ion;"
         xmlns:comp="&comp;"
         xmlns:prop="&prop;"
         xmlns:quan="&quan;"
         xmlns:indx="&indx;"
         xmlns:repr="&repr;"
         xmlns:units="&units;"
         xmlns:chem="&chem;"
         xmlns:conc="&conc;"
         xmlns:chemp="&chemp;"
         xmlns:elem="&elem;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
 	 xmlns:xsd="&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&chem;"/>
     <owl:imports rdf:resource="&comp;"/>
     <owl:imports rdf:resource="&conc;"/>
     <owl:imports rdf:resource="&prop;"/>
     <owl:imports rdf:resource="&quan;"/>
     <owl:imports rdf:resource="&cstate;"/>
     <owl:imports rdf:resource="&pstate;"/>
     <owl:imports rdf:resource="&screla;"/>
     <owl:imports rdf:resource="&crela;"/>
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&ion;"/>
     <owl:imports rdf:resource="&elem;"/>
     <owl:imports rdf:resource="&repr;"/>
     <owl:imports rdf:resource="&indx;"/>
     <owl:imports rdf:resource="&units;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#oxygendemand">
    <rdfs:subClassOf rdf:resource="&conc;#densiti"/>
  </owl:Class>

  <owl:Class rdf:about="#biologicaloxygendemand">
    <rdfs:subClassOf rdf:resource="#oxygendemand"/>
     <owl:equivalentClass rdf:resource="#biochemicaloxygendemand" />
     <owl:equivalentClass rdf:resource="#bod" />
  </owl:Class>
  <owl:Class rdf:about="#biochemicaloxygendemand"/>
  <owl:Class rdf:about="#bod"/>

  <owl:Class rdf:about="#chemicaloxygendemand">
    <rdfs:subClassOf rdf:resource="#oxygendemand"/>
     <owl:equivalentClass rdf:resource="#cod" />
  </owl:Class>
  <owl:Class rdf:about="#cod"/>

  <owl:Class rdf:about="#saturationpoint">
    <rdfs:subClassOf rdf:resource="#saturationproperti"/>
  </owl:Class>

  <owl:Class rdf:about="#saturationproperti">
    <rdfs:subClassOf rdf:resource="&prop;#properti"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasproperti" />
        <owl:hasValue rdf:resource="&cstate;#satur" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#puriti">
    <rdfs:subClassOf rdf:resource="&prop;#chemicalproperti"/>
  </owl:Class>

  <owl:Class rdf:about="#aciditi">
    <rdfs:subClassOf rdf:resource="&prop;#chemicalproperti"/>
  </owl:Class>

  <owl:Class rdf:about="#alkalin">
    <rdfs:subClassOf rdf:resource="&prop;#chemicalproperti"/>
    <owl:equivalentClass rdf:resource="#basic"/>
    <owl:equivalentClass rdf:resource="#basic"/>
    <owl:equivalentClass rdf:resource="#alkalic"/>
    <owl:equivalentClass rdf:resource="#alkalin"/>
  </owl:Class>
  <owl:Class rdf:about="#basic"/>
  <owl:Class rdf:about="#basic"/>
  <owl:Class rdf:about="#alkalic"/>
  <owl:Class rdf:about="#alkalin"/>

  <owl:Class rdf:about="#carbonatealkalin">
    <rdfs:subClassOf rdf:resource="#alkalin"/>
  </owl:Class>

  <owl:Class rdf:about="#composit">
    <rdfs:subClassOf rdf:resource="&prop;#chemicalproperti"/>
  </owl:Class>

  <owl:Class rdf:about="#hplusconcentr">
    <rdfs:subClassOf rdf:resource="&quan;#chemicalqu"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&crela;#hassubst"/>
        <owl:hasValue rdf:resource="&ion;#hplus" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ph">
    <rdfs:subClassOf rdf:resource="&quan;#chemicalqu"/>
    <rdfs:comment xml:lang="en">pH is a measure of the acidity or basicity of a solution. It is defined as the cologarithm of the activity of dissolved hydrogen ions (H+). Hydrogen ion activity coefficients cannot be measured experimentally, so they are based on theoretical calculations. The pH scale is not an absolute scale; it is relative to a set of standard solutions whose pH is established by international agreement. [Wikipedia]</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#totalalkalin">
    <rdfs:subClassOf rdf:resource="#alkalin"/>
  </owl:Class>

  <owl:Class rdf:about="#reactiv">
    <rdfs:subClassOf rdf:resource="&prop;#reactionproperti"/>
  </owl:Class>

  <owl:Class rdf:about="#solubl">
    <rdfs:subClassOf rdf:resource="&prop;#reactionproperti"/>
    <rdfs:comment xml:lang="en">The total amount of solute species that will remain indefinitely in a solution maintained at constant temperature and pressure in contact with the solid crystals from which the solutes were derived.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#salin">
    <rdfs:subClassOf rdf:resource="&conc;#massconcentr"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&ion;#saltion"/>
        <owl:onProperty rdf:resource="&crela;#hassubst"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#permil"/>
        <owl:onProperty rdf:resource="&screla;#hasdefaultunit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#sodic">
    <rdfs:subClassOf rdf:resource="&conc;#massconcentr"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&elem;#sodium"/>
        <owl:onProperty rdf:resource="&crela;#hassubst"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ioncomposit">
    <rdfs:subClassOf rdf:resource="#composit"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&pstate;#ionizedst"/>
        <owl:onProperty rdf:resource="&rela;#hasstat"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#zoomass">
    <rdfs:subClassOf rdf:resource="&conc;#dryweight"/>
  </owl:Class>

<!-- Hydro properties -->
  <owl:Class rdf:about="#moistur">
    <rdfs:subClassOf rdf:resource="#waterconcentr"/>
  </owl:Class>

  <owl:Class rdf:about="#waterconcentr">
    <rdfs:subClassOf rdf:resource="&conc;#massconcentr"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&crela;#hassubst" />
        <owl:hasValue rdf:resource="&comp;#water" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#sodiumadsorptionratio">
    <rdfs:subClassOf rdf:resource="&indx;#waterqualityindex" />
    <owl:equivalentClass rdf:resource="#sar"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#sodic"/>
        <owl:onProperty rdf:resource="&screla;#measureof"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#sar"/>

  <owl:Class rdf:about="#totaldissolvedsolid">
    <rdfs:subClassOf rdf:resource="&conc;#densiti"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&chem;#dissolvedsolid"/>
        <owl:onProperty rdf:resource="&crela;#hassubst"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#totalsuspendedsolid">
    <rdfs:subClassOf rdf:resource="&conc;#densiti"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&chem;#suspendedsolid"/>
        <owl:onProperty rdf:resource="&crela;#hassubst"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#load">
    <rdfs:subClassOf rdf:resource="&conc;#densiti"/>
  </owl:Class>

  <owl:Class rdf:about="#bedload">
    <rdfs:subClassOf rdf:resource="#load"/>
    <owl:disjointWith rdf:resource="#suspendedload"/>
  </owl:Class>

  <owl:Class rdf:about="#dissolvedload">
    <rdfs:subClassOf rdf:resource="#load"/>
    <rdfs:subClassOf rdf:resource="&chem;#dissolvedsolid"/>
  </owl:Class>

  <owl:Class rdf:about="#suspendedload">
    <rdfs:subClassOf rdf:resource="#load"/>
    <rdfs:subClassOf rdf:resource="&chem;#suspendedsolid"/>
    <owl:equivalentClass rdf:resource="#suspensionload" />
  </owl:Class>
  <owl:Class rdf:about="#suspensionload"/>

  <owl:Class rdf:about="#washload">
    <rdfs:subClassOf rdf:resource="#load"/>
    <owl:disjointWith rdf:resource="#suspendedload"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat" />
        <owl:hasValue rdf:resource="&pstate;#float" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#abundanc">
    <rdfs:subClassOf rdf:resource="#composit"/>
  </owl:Class>

  <owl:Class rdf:about="#constitu">
    <rdfs:subClassOf rdf:resource="&conc;#consistenceproperti"/>
    <rdfs:subClassOf rdf:resource="&repr;#compon"/>
  </owl:Class>

</rdf:RDF>
