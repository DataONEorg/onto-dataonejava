<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
   <!ENTITY func "http://sweet.jpl.nasa.gov/2.3/reprMathFunctionOrthogonal.owl">   
   <!ENTITY serv "http://sweet.jpl.nasa.gov/2.3/reprDataService.owl">   
   <!ENTITY anal "http://sweet.jpl.nasa.gov/2.3/reprDataServiceAnalysis.owl#">   
   <!ENTITY mod  "http://sweet.jpl.nasa.gov/2.3/reprSciModel.owl">   
   <!ENTITY solu "http://sweet.jpl.nasa.gov/2.3/reprMathSolution.owl">   
   <!ENTITY human "http://sweet.jpl.nasa.gov/2.3/human.owl">
   <!ENTITY prrela "http://sweet.jpl.nasa.gov/2.3/relaProvenance.owl">
   <!ENTITY mrela "http://sweet.jpl.nasa.gov/2.3/relaMath.owl">
   <!ENTITY owl "http://www.w3.org/2002/07/owl#">
   <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
   <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF
    xmlns = "&anal;"
    xml:base = "&anal;"
    xmlns:serv = "&serv;"
    xmlns:anal = "&anal;"
    xmlns:func = "&func;"
    xmlns:mod  = "&mod;"
    xmlns:solu = "&solu;"
    xmlns:human = "&human;"
    xmlns:prrela = "&prrela;"
    xmlns:mrela = "&mrela;"
    xmlns:owl = "&owl;"
    xmlns:rdf = "&rdf;"
    xmlns:rdfs = "&rdfs;"
    xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
    <owl:imports rdf:resource = "&human;"/>
    <owl:imports rdf:resource = "&prrela;"/>
    <owl:imports rdf:resource = "&mrela;"/>
    <owl:imports rdf:resource = "&solu;"/>
    <owl:imports rdf:resource = "&func;"/>
    <owl:imports rdf:resource = "&mod;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#dataanalysisserv">
    <rdfs:subClassOf rdf:resource="&serv;#dataserv"/>
  </owl:Class>

<!-- Parameterizion -->
  <owl:Class rdf:about="#paramet">
    <rdfs:subClassOf rdf:resource="#dataanalysisserv"/>
    <owl:equivalentClass rdf:resource="&mod;#paramet"/>
  </owl:Class>

  <owl:Class rdf:about="#assimil">
    <rdfs:subClassOf rdf:resource="#paramet"/>
    <owl:equivalentClass rdf:resource="#assimil"/>
  </owl:Class>
  <owl:Class rdf:about="#assimil"/>

  <owl:Class rdf:about="#associationrulelearn">
    <rdfs:subClassOf rdf:resource="#paramet"/>
  </owl:Class>

  <owl:Class rdf:about="#fit">
    <rdfs:subClassOf rdf:resource="#paramet"/>
    <owl:equivalentClass rdf:resource="#fit"/>
  </owl:Class>
  <owl:Class rdf:about="#fit"/>

  <owl:Class rdf:about="#train">
    <rdfs:subClassOf rdf:resource="#paramet"/>
    <owl:equivalentClass rdf:resource="#train"/>
  </owl:Class>
  <owl:Class rdf:about="#train"/>

<!-- Data classification -->
  <owl:Class rdf:about="#classif">
    <rdfs:subClassOf rdf:resource="#dataanalysisserv"/>
    <owl:equivalentClass rdf:resource="#classifi"/>
    <owl:equivalentClass rdf:resource="#classifi"/>
  </owl:Class>
  <owl:Class rdf:about="#classifi"/>
  <owl:Class rdf:about="#classifi"/>

  <anal:Classifier rdf:about="#decisiontreelearn">
  </anal:Classifier>

  <anal:Classifier rdf:about="#naivebay">
  </anal:Classifier>

  <anal:Classifier rdf:about="#neuralnetwork">
  </anal:Classifier>

  <owl:Class rdf:about="#cluster">
    <rdfs:subClassOf rdf:resource="#dataanalysisserv"/>
    <owl:equivalentClass rdf:resource="#cluster"/>
  </owl:Class>
  <owl:Class rdf:about="#cluster"/>

<!-- Spectral analysis -->
  <owl:Class rdf:about="#filter">
    <rdfs:subClassOf rdf:resource="#spectralanalysi"/>
  </owl:Class>

  <owl:Class rdf:about="#spectrum">
    <rdfs:subClassOf rdf:resource="#spectralanalysi"/>
    <owl:equivalentClass rdf:resource="#powerspectrum"/>
  </owl:Class>
  <owl:Class rdf:about="#powerspectrum"/>

  <owl:Class rdf:about="#fourieranalysi">
    <rdfs:subClassOf rdf:resource="#spectralanalysi"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&func;#fourierfunct"/>
        <owl:onProperty rdf:resource="&mrela;#hasbasisfunct"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass rdf:resource="#fouriertransform"/>
  </owl:Class>
  <owl:Class rdf:about="#fouriertransform"/>

  <owl:Class rdf:about="#spectralanalysi">
    <rdfs:subClassOf rdf:resource="#dataanalysisserv"/>
    <owl:equivalentClass rdf:resource="#frequencydomainanalysi"/>
  </owl:Class>
  <owl:Class rdf:about="#frequencydomainanalysi"/>

  <owl:Class rdf:about="#spectralsynthesi">
    <rdfs:subClassOf rdf:resource="#dataanalysisserv"/>
  </owl:Class>

  <owl:Class rdf:about="#sphericalharmonicanalysi">
    <rdfs:subClassOf rdf:resource="#spectralanalysi"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&func;#sphericalharmonicfunct"/>
        <owl:onProperty rdf:resource="&mrela;#hasbasisfunct"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#waveletanalysi">
    <rdfs:subClassOf rdf:resource="#spectralanalysi"/>
  </owl:Class>

<!-- Scientific processing -->
  <owl:Class rdf:about="#reanalyz">
    <rdfs:subClassOf rdf:resource="#dataanalysisserv"/>
    <owl:equivalentClass rdf:resource="#reanalysi"/>
  </owl:Class>
  <owl:Class rdf:about="#reanalysi"/>

  <owl:Class rdf:about="#reconstruct">
    <rdfs:subClassOf rdf:resource="#dataanalysisserv"/>
    <owl:equivalentClass rdf:resource="#reconstruct"/>
  </owl:Class>
  <owl:Class rdf:about="#reconstruct"/>

  <owl:Class rdf:about="#timeseri">
    <rdfs:subClassOf rdf:resource="#dataanalysisserv"/>
  </owl:Class>

  <owl:Class rdf:about="#knowledgediscoveri">
    <rdfs:subClassOf rdf:resource="#dataanalysisserv"/>
  </owl:Class>

<!-- Interpolation -->
  <owl:Class rdf:about="#interpol">
    <rdfs:subClassOf rdf:resource="&serv;#dataserv"/>
    <owl:equivalentClass rdf:resource="#interpol"/>
  </owl:Class>
  <owl:Class rdf:about="#interpol"/>

  <owl:Class rdf:about="#interpolationmethod">
    <rdfs:subClassOf rdf:resource="&solu;#algorithm"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#interpol"/>
        <owl:onProperty rdf:resource="&prrela;#hasdataserv"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <anal:InterpolationMethod  rdf:about="#distanceweight">
  </anal:InterpolationMethod>

  <anal:InterpolationMethod  rdf:about="#nearestneighbor">
  </anal:InterpolationMethod>

<!-- Visualization -->
  <owl:Class rdf:about="#visual">
    <rdfs:subClassOf rdf:resource="#dataanalysisserv"/>
    <owl:equivalentClass rdf:resource="#visual"/>
  </owl:Class>
  <owl:Class rdf:about="#visual"/>

  <owl:Class rdf:about="#animat">
    <rdfs:subClassOf rdf:resource="#visual"/>
    <owl:equivalentClass rdf:resource="#animat"/>
  </owl:Class>
  <owl:Class rdf:about="#animat"/>

  <owl:Class rdf:about="#overlay">
    <rdfs:subClassOf rdf:resource="#visual"/>
  </owl:Class>

  <owl:Class rdf:about="#superimpo">
    <rdfs:subClassOf rdf:resource="#visual"/>
  </owl:Class>
  
  <owl:Class rdf:about="#plot">
    <rdfs:subClassOf rdf:resource="#visual"/>
  </owl:Class>

  <owl:Class rdf:about="#scattergram">
    <rdfs:subClassOf rdf:resource="#plot"/>
  </owl:Class>

</rdf:RDF>
