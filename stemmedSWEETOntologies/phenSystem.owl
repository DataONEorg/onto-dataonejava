<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
   <!ENTITY sys "http://sweet.jpl.nasa.gov/2.3/phenSystem.owl">   
   <!ENTITY mod "http://sweet.jpl.nasa.gov/2.3/reprSciModel.owl">   
   <!ENTITY phen "http://sweet.jpl.nasa.gov/2.3/phen.owl">
   <!ENTITY owl "http://www.w3.org/2002/07/owl#">
   <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
   <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF
    xml:base = "&sys;"
    xmlns:phen = "&phen;"
    xmlns:sys = "&sys;"
    xmlns:mod = "&mod;"
    xmlns:owl = "&owl;"
    xmlns:rdf = "&rdf;"
    xmlns:rdfs = "&rdfs;"
    xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
    <owl:imports rdf:resource = "&mod;"/>
    <owl:imports rdf:resource = "&phen;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#system">
    <rdfs:subClassOf rdf:resource="&mod;#model"/>
    <rdfs:comment> System is a set of interacting or interdependent entities, real or abstract, forming an integrated whole.
    </rdfs:comment>
  </owl:Class>
  
  <owl:Class rdf:about="#chang">
    <rdfs:subClassOf rdf:resource="#systemphenomena" />
  </owl:Class>

  <owl:Class rdf:about="#displac">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#disrupt">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#disturb">
    <rdfs:subClassOf rdf:resource="#variat" />
  </owl:Class>

  <owl:Class rdf:about="#exchang">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#flow">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#interact">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
    <rdfs:comment>process that multiple substances are in contact and have some consequence</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#interrupt">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#systemstatechang">
    <rdfs:subClassOf rdf:resource="#chang"/>
  </owl:Class>

  <owl:Class rdf:about="#demand">
    <rdfs:subClassOf rdf:resource="#systemphenomena"/>
  </owl:Class>

  <owl:Class rdf:about="#systemphenomena">
    <rdfs:subClassOf rdf:resource="&phen;#phenomena"/>
  </owl:Class>

  <owl:Class rdf:about="#drift">
    <rdfs:subClassOf rdf:resource="#trend" />
  </owl:Class>

  <owl:Class rdf:about="#shift">
    <rdfs:subClassOf rdf:resource="#trend" />
  </owl:Class>

  <owl:Class rdf:about="#trend">
    <rdfs:subClassOf rdf:resource="#systemphenomena" />
  </owl:Class>

  <owl:Class rdf:about="#lag">
    <rdfs:subClassOf rdf:resource="#systemphenomena" />
  </owl:Class>

  <owl:Class rdf:about="#stress">
    <rdfs:subClassOf rdf:resource="#systemphenomena"/>
  </owl:Class>

  <owl:Class rdf:about="#conver">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

<!-- System growth -->
  <owl:Class rdf:about="#return">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#comeback">
    <rdfs:subClassOf rdf:resource="#return"/>
  </owl:Class>

  <owl:Class rdf:about="#accumul">
    <rdfs:subClassOf rdf:resource="#systemphenomena" />
  </owl:Class>

  <owl:Class rdf:about="#deposit">
    <rdfs:subClassOf rdf:resource="#accumul" />
  </owl:Class>

  <owl:Class rdf:about="#extension">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#inundat">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#growth">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>
  
  <owl:Class rdf:about="#increa">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
    <owl:equivalentClass rdf:resource="#rise"/>
  </owl:Class>
  <owl:Class rdf:about="#rise"/>
  
  <owl:Class rdf:about="#replenish">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#collect">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

<!-- System loss -->
  <owl:Class rdf:about="#remov">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#loss">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#destruct">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#decrea">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
    <owl:equivalentClass rdf:resource="#drop"/>
    <owl:equivalentClass rdf:resource="#fall"/>
    <owl:equivalentClass rdf:resource="#declin"/>
    <owl:equivalentClass rdf:resource="#reduct"/>
  </owl:Class>
  <owl:Class rdf:about="#drop"/>
  <owl:Class rdf:about="#fall"/>
  <owl:Class rdf:about="#declin"/>
  <owl:Class rdf:about="#reduct"/>
  
  <owl:Class rdf:about="#halt">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>
  
  <owl:Class rdf:about="#retreat">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>
  
  <owl:Class rdf:about="#oscillat">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>
  
  <owl:Class rdf:about="#perturb">
    <rdfs:subClassOf rdf:resource="#variat" />
    <rdfs:comment xml:lang="en">Any departure introduced into an assumed steady state of a system.   The magnitude is often assumed to be small so that product terms in the dependent variables may be neglected.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#vacil">
    <rdfs:subClassOf rdf:resource="#oscillat" />
    <rdfs:comment xml:lang="en">Oscillation, usually of a small amplitude, about a reference state.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#variabl">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#variat">
    <rdfs:subClassOf rdf:resource="#systemstatechang"/>
  </owl:Class>

  <owl:Class rdf:about="#recycl">
    <rdfs:subClassOf rdf:resource="#systemphenomena" />
  </owl:Class>

</rdf:RDF>
