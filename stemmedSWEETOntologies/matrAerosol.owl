<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY land "http://sweet.jpl.nasa.gov/2.3/realmLandform.owl">
  <!ENTITY rela "http://sweet.jpl.nasa.gov/2.3/rela.owl">
  <!ENTITY chem "http://sweet.jpl.nasa.gov/2.3/matr.owl">
  <!ENTITY aero "http://sweet.jpl.nasa.gov/2.3/matrAerosol.owl">
  <!ENTITY pstate "http://sweet.jpl.nasa.gov/2.3/statePhysical.owl">
  <!ENTITY xten "http://sweet.jpl.nasa.gov/2.3/propSpaceDistance.owl">
  <!ENTITY xten2 "http://sweet.jpl.nasa.gov/2.3/propSpaceDistance.owl#">
  <!ENTITY srela "http://sweet.jpl.nasa.gov/2.3/relaSpace.owl">
  <!ENTITY mrela "http://sweet.jpl.nasa.gov/2.3/relaMath.owl">
  <!ENTITY mrela2 "http://sweet.jpl.nasa.gov/2.3/relaMath.owl#">
  <!ENTITY solid "http://sweet.jpl.nasa.gov/2.3/phenSolid.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&aero;"
         xmlns:owl="&owl;"
         xmlns:land="&land;"
         xmlns:aero="&aero;"
         xmlns:rela="&rela;"
         xmlns:pstate="&pstate;"
         xmlns:solid="&solid;"
         xmlns:chem="&chem;"
         xmlns:xten="&xten;"
         xmlns:xten2="&xten2;"
         xmlns:srela="&srela;"
         xmlns:mrela="&mrela;"
         xmlns:mrela2="&mrela2;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
 	 xmlns:xsd="&xsd;">

<!-- Ontology Information  -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&chem;"/>
     <owl:imports rdf:resource="&solid;"/>
     <owl:imports rdf:resource="&land;"/>
     <owl:imports rdf:resource="&mrela;"/>
     <owl:imports rdf:resource="&srela;"/>
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&xten;"/>
     <owl:imports rdf:resource="&pstate;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#aerosol">
    <rdfs:subClassOf rdf:resource="&chem;#suspendedsubst"/>
  </owl:Class>

  <owl:Class rdf:about="#continentalaerosol">
    <rdfs:subClassOf rdf:resource="#aerosol" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasrealm" />
        <owl:allValuesFrom rdf:resource="&land;#land" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Aerosol having its origin over the continents with industrial, urban, agricultural, forest, and desert sources, with potential for high concentrations of hygroscopic aerosol.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#fugitivedust">
    <rdfs:subClassOf rdf:resource="#dust"/>
  </owl:Class>

  <owl:Class rdf:about="#pm1">
    <rdfs:subClassOf rdf:resource="#particul"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#diamet"/>
        <owl:hasValue>
          <xten2:SizeRange_micron>
            <mrela2:hasUpperBound rdf:datatype="&xsd;double">1</mrela2:hasUpperBound>
          </xten2:SizeRange_micron>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#pm2point5">
    <rdfs:subClassOf rdf:resource="#particul"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#diamet"/>
        <owl:hasValue>
          <xten2:SizeRange_micron>
            <mrela2:hasUpperBound rdf:datatype="&xsd;double">2.5</mrela2:hasUpperBound>
          </xten2:SizeRange_micron>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#pm10">
    <rdfs:subClassOf rdf:resource="#particul"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#diamet"/>
        <owl:hasValue>
          <xten2:SizeRange_micron>
            <mrela2:hasUpperBound rdf:datatype="&xsd;double">10</mrela2:hasUpperBound>
          </xten2:SizeRange_micron>
        </owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#particul">
    <rdfs:subClassOf rdf:resource="&chem;#suspendedsubst"/>
    <rdfs:comment xml:lang="en">The term for solid or liquid particles found in a gas.   Some particles are large or dark enough to be seen as soot or smoke.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#pollen">
    <rdfs:subClassOf rdf:resource="#particul"/>
  </owl:Class>

  <owl:Class rdf:about="#smoke">
    <rdfs:subClassOf rdf:resource="&chem;#suspendedsubst"/>
  </owl:Class>

  <owl:Class rdf:about="#spray">
    <rdfs:subClassOf rdf:resource="#aerosol"/>
  </owl:Class>

  <owl:Class rdf:about="#soot">
    <rdfs:subClassOf rdf:resource="#particul"/>
    <owl:equivalentClass rdf:resource="#blackcarbon" />
  </owl:Class>
  <owl:Class rdf:about="#blackcarbon"/>

  <owl:Class rdf:about="#ash">
    <rdfs:subClassOf rdf:resource="&chem;#solidsubst"/>
   <rdfs:comment>Fine particles of pulverized rock.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#dust">
    <rdfs:subClassOf rdf:resource="&chem;#solidsubst"/>
  </owl:Class>

  <owl:Class rdf:about="#condensationnuclei">
    <rdfs:subClassOf rdf:resource="&chem;#solidsubst"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&solid;#coalesc"/>
        <owl:onProperty rdf:resource="&rela;#hasrol"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#cloudcondensationnuclei">
    <rdfs:subClassOf rdf:resource="#condensationnuclei"/>
    <owl:equivalentClass rdf:resource="#ccn" />
  </owl:Class>
  <owl:Class rdf:about="#ccn"/>

  <owl:Class rdf:about="#grain">
    <rdfs:subClassOf rdf:resource="&pstate;#substanceform"/>
    <rdfs:subClassOf rdf:resource="&chem;#solidsubst"/>
  </owl:Class>

</rdf:RDF>
