<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
   <!ENTITY serv "http://sweet.jpl.nasa.gov/2.3/reprDataService.owl">   
   <!ENTITY res "http://sweet.jpl.nasa.gov/2.3/humanResearch.owl">   
   <!ENTITY vali "http://sweet.jpl.nasa.gov/2.3/reprDataServiceValidation.owl#">   
   <!ENTITY owl "http://www.w3.org/2002/07/owl#">
   <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
   <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF
    xmlns = "&vali;"
    xml:base = "&vali;"
    xmlns:serv = "&serv;"
    xmlns:vali = "&vali;"
    xmlns:res = "&res;"
    xmlns:owl = "&owl;"
    xmlns:rdf = "&rdf;"
    xmlns:rdfs = "&rdfs;"
    xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
    <owl:imports rdf:resource = "&serv;"/>
    <owl:imports rdf:resource = "&res;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#dataclean">
    <rdfs:subClassOf rdf:resource="#verif"/>
  </owl:Class>

  <owl:Class rdf:about="#mask">
    <rdfs:subClassOf rdf:resource="&serv;#dataserv"/>
    <owl:equivalentClass rdf:resource="#binarymask"/>
  </owl:Class>
  <owl:Class rdf:about="#binarymask"/>

  <owl:Class rdf:about="#acceptanc">
    <rdfs:subClassOf rdf:resource="&serv;#dataserv"/>
  </owl:Class>

  <owl:Class rdf:about="#pixelscreen">
    <rdfs:subClassOf rdf:resource="#qualityscreen"/>
  </owl:Class>

  <owl:Class rdf:about="#qualityscreen">
    <rdfs:subClassOf rdf:resource="#verif"/>
  </owl:Class>

  <owl:Class rdf:about="#valid">
    <rdfs:subClassOf rdf:resource="#verif"/>
  </owl:Class>

  <owl:Class rdf:about="#verifi">
    <rdfs:subClassOf rdf:resource="&serv;#dataserv"/>
    <owl:equivalentClass rdf:resource="#verif"/>
  </owl:Class>
  <owl:Class rdf:about="#verif" />

  <owl:Class rdf:about="#calibr">
    <rdfs:subClassOf rdf:resource="#verif"/>
