<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY math "http://sweet.jpl.nasa.gov/2.3/reprMath.owl">
  <!ENTITY rdir "http://sweet.jpl.nasa.gov/2.3/reprSpaceDirection.owl">
  <!ENTITY dir "http://sweet.jpl.nasa.gov/2.3/propSpaceDirection.owl#">  
  <!ENTITY loca "http://sweet.jpl.nasa.gov/2.3/propSpaceLocation.owl">  
  <!ENTITY obj3d "http://sweet.jpl.nasa.gov/2.3/reprSpaceGeometry3D.owl">  
  <!ENTITY srela "http://sweet.jpl.nasa.gov/2.3/relaSpace.owl">  
  <!ENTITY screla "http://sweet.jpl.nasa.gov/2.3/relaSci.owl">  
  <!ENTITY quan "http://sweet.jpl.nasa.gov/2.3/propQuantity.owl">
  <!ENTITY units "http://sweet.jpl.nasa.gov/2.3/reprSciUnits.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&dir;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:srela="&srela;"
         xmlns:screla="&screla;"
         xmlns:obj3d="&obj3d;"
         xmlns:math="&math;"
         xmlns:quan="&quan;"
         xmlns:units="&units;"
         xmlns:loca="&loca;"
         xmlns:dir="&dir;"
         xmlns:rdir="&rdir;"
>
<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&math;"/>
     <owl:imports rdf:resource="&loca;"/>
     <owl:imports rdf:resource="&rdir;"/>
     <owl:imports rdf:resource="&srela;"/>
     <owl:imports rdf:resource="&screla;"/>
     <owl:imports rdf:resource="&obj3d;"/>
     <owl:imports rdf:resource="&quan;"/>
     <owl:imports rdf:resource="&units;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#Direct">
    <rdfs:subClassOf rdf:resource="&math;#Vector_3D"/>
  </owl:Class>
  
  <owl:Class rdf:about="#AngularDirect">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#Attitud">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#Axis">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#Azimuth">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#AzimuthalDirect">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#Bear">
    <owl:equivalentClass rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#HorizontalDirect">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#Incidenc">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#Inclinat">
    <owl:equivalentClass rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#Orientat">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#PolarDirect">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#SolidAngl">
    <rdfs:subClassOf rdf:resource="&quan;#Quantiti"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#steradian"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&obj3d;#Cone"/>
        <owl:onProperty rdf:resource="&srela;#hasGeometricalObject"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:about="#Tilt">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#VerticalDirect">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#Angle">
    <rdfs:subClassOf rdf:resource="&quan;#Quantiti"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#radian"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#hasDirect"/>
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Income">
    <rdfs:subClassOf rdf:resource="#Direct"/>
    <owl:equivalentClass rdf:resource="#Inward"/>
  </owl:Class>
  <owl:Class rdf:about="#Inward"/>
  
  <owl:Class rdf:about="#Later">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#Outgo">
    <rdfs:subClassOf rdf:resource="#Direct"/>
    <owl:equivalentClass rdf:resource="#Outward"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Income"/>
        <owl:onProperty rdf:resource="&srela;#oppositeTo"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Outward"/>
  
  <owl:Class rdf:about="#Radial">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

  <owl:Class rdf:about="#Zenith">
    <rdfs:subClassOf rdf:resource="#Direct"/>
  </owl:Class>

</rdf:RDF>
