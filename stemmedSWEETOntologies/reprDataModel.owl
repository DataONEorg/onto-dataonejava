<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
   <!ENTITY info "http://sweet.jpl.nasa.gov/2.3/reprDataModel.owl">   
   <!ENTITY time "http://sweet.jpl.nasa.gov/2.3/reprTime.owl">   
   <!ENTITY data "http://sweet.jpl.nasa.gov/2.3/reprDataProduct.owl">   
   <!ENTITY space "http://sweet.jpl.nasa.gov/2.3/reprSpace.owl">   
   <!ENTITY mod "http://sweet.jpl.nasa.gov/2.3/reprSciModel.owl">   
   <!ENTITY repr "http://sweet.jpl.nasa.gov/2.3/repr.owl">   
   <!ENTITY mrela "http://sweet.jpl.nasa.gov/2.3/relaMath.owl">   
   <!ENTITY srela "http://sweet.jpl.nasa.gov/2.3/relaSpace.owl">   
   <!ENTITY obj "http://sweet.jpl.nasa.gov/2.3/reprSpaceGeometry.owl">
   <!ENTITY obj3d "http://sweet.jpl.nasa.gov/2.3/reprSpaceGeometry3D.owl">
   <!ENTITY owl "http://www.w3.org/2002/07/owl#">
   <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
   <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF
    xmlns = "&info;"
    xml:base = "&info;"
    xmlns:info = "&info;"
    xmlns:repr = "&repr;"
    xmlns:obj = "&obj;"
    xmlns:obj3d = "&obj3d;"
    xmlns:mrela = "&mrela;"
    xmlns:srela = "&srela;"
    xmlns:mod = "&mod;"
    xmlns:time = "&time;"
    xmlns:data = "&data;"
    xmlns:space = "&space;"
    xmlns:owl = "&owl;"
    xmlns:rdf = "&rdf;"
    xmlns:rdfs = "&rdfs;"
    xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
    <owl:imports rdf:resource = "&obj;"/>
    <owl:imports rdf:resource = "&obj3d;"/>
    <owl:imports rdf:resource = "&mod;"/>
    <owl:imports rdf:resource = "&space;"/>
    <owl:imports rdf:resource = "&srela;"/>
    <owl:imports rdf:resource = "&mrela;"/>
    <owl:imports rdf:resource = "&time;"/>
    <owl:imports rdf:resource = "&data;"/>
    <owl:imports rdf:resource = "&repr;"/>
  </owl:Ontology>

<!-- Classes -->
  <owl:Class rdf:about="#array">
    <rdfs:subClassOf rdf:resource="#datastructur"/>
  </owl:Class>

  <owl:Class rdf:about="#cell">
    <rdfs:subClassOf rdf:resource="#datamodel"/>
  </owl:Class>

  <owl:Class rdf:about="#charact">
    <rdfs:subClassOf rdf:resource="&repr;#repres"/>
  </owl:Class>

  <owl:Class rdf:about="#column">
    <rdfs:subClassOf rdf:resource="&repr;#dimen"/>
  </owl:Class>

  <owl:Class rdf:about="#datamodel">
    <rdfs:subClassOf rdf:resource="&mod;#model" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&mrela;#representationof" />
        <owl:allValuesFrom rdf:resource="&data;#data" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#datastructur">
    <rdfs:subClassOf rdf:resource="&repr;#repres" />
  </owl:Class>

  <owl:Class rdf:about="#delimit">
    <rdfs:subClassOf rdf:resource="#charact"/>
  </owl:Class>

  <owl:Class rdf:about="#field">
    <rdfs:subClassOf rdf:resource="#datamodel" />
  </owl:Class>

  <owl:Class rdf:about="#grid">
    <rdfs:subClassOf rdf:resource="#datamodel"/>
    <rdfs:subClassOf rdf:resource="#raster"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#hasgeometricalobject" />
        <owl:someValuesFrom rdf:resource="&obj;#rectangl" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#increment">
    <rdfs:subClassOf rdf:resource="&repr;#scale"/>
  </owl:Class>

  <owl:Class rdf:about="#point">
    <rdfs:subClassOf rdf:resource="#datamodel"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#hasgeometricalobject" />
        <owl:allValuesFrom rdf:resource="&obj;#point" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#raster">
    <rdfs:subClassOf rdf:resource="#datamodel"/>
  </owl:Class>

  <owl:Class rdf:about="#reducedgrid">
    <rdfs:subClassOf rdf:resource="#grid"/>
  </owl:Class>
  
  <owl:Class rdf:about="#resolut">
    <rdfs:subClassOf rdf:resource="#increment"/>
  </owl:Class>

  <owl:Class rdf:about="#spatialrepres">
    <rdfs:subClassOf rdf:resource="&repr;#repres" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&mrela;#representationof" />
        <owl:allValuesFrom rdf:resource="&space;#space" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#row">
    <rdfs:subClassOf rdf:resource="&repr;#dimen"/>
  </owl:Class>

  <owl:Class rdf:about="#structur">
    <rdfs:subClassOf rdf:resource="#datastructur"/>
  </owl:Class>

  <owl:Class rdf:about="#swath">
    <rdfs:subClassOf rdf:resource="#datamodel"/>
  </owl:Class>

  <owl:Class rdf:about="#Swath_2D">
    <rdfs:subClassOf rdf:resource="#swath"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#hasgeometricalobject" />
        <owl:allValuesFrom rdf:resource="&obj;#GeometricalObject_2D" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Swath_3D">
    <rdfs:subClassOf rdf:resource="#swath"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#hasgeometricalobject" />
        <owl:allValuesFrom rdf:resource="&obj3d;#GeometricalObject_3D" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#temporalrepres">
    <rdfs:subClassOf rdf:resource="&repr;#repres" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&mrela;#representationof" />
        <owl:allValuesFrom rdf:resource="&time;#time" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#trajectori">
    <rdfs:subClassOf rdf:resource="#datamodel"/>
  </owl:Class>

  <owl:Class rdf:about="#vector">
    <rdfs:subClassOf rdf:resource="#datamodel"/>
  </owl:Class>

  <owl:Class rdf:about="#paramet">
    <rdfs:subClassOf rdf:resource="&repr;#dimen"/>
  </owl:Class>

</rdf:RDF>
