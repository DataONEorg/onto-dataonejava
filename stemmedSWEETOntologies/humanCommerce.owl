<!-- Extraction -->
  <owl:Class rdf:about="#Dig">
    <rdfs:subClassOf rdf:resource="#Extraction"/>
  </owl:Class>

  <owl:Class rdf:about="#Drill">
    <rdfs:subClassOf rdf:resource="#Extraction"/>
  </owl:Class>

  <owl:Class rdf:about="#Explorat">
    <rdfs:subClassOf rdf:resource="#EngineeringAct"/>
  </owl:Class>

  <owl:Class rdf:about="#Extraction">
    <rdfs:subClassOf rdf:resource="#EngineeringAct"/>
    <owl:equivalentClass rdf:resource="#ResourceExtract"/>
  </owl:Class>
  <owl:Class rdf:about="#ResourceExtract"/>

  <owl:Class rdf:about="#Mine">
    <rdfs:subClassOf rdf:resource="#Extraction"/>
  </owl:Class>

  <owl:Class rdf:about="#Prospect">
    <rdfs:subClassOf rdf:resource="#Explorat"/>
  </owl:Class>

  <owl:Class rdf:about="#StripMin">
    <rdfs:subClassOf rdf:resource="#Mine"/>
  </owl:Class>

</rdf:RDF>
