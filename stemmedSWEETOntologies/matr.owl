<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY role  "http://sweet.jpl.nasa.gov/2.3/stateRole.owl">
  <!ENTITY rela  "http://sweet.jpl.nasa.gov/2.3/rela.owl">
  <!ENTITY chem "http://sweet.jpl.nasa.gov/2.3/matr.owl">
  <!ENTITY pstate "http://sweet.jpl.nasa.gov/2.3/statePhysical.owl">
  <!ENTITY state "http://sweet.jpl.nasa.gov/2.3/state.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&chem;"
         xmlns:chem="&chem;"
         xmlns:owl="&owl;"
         xmlns:role="&role;"
         xmlns:rela="&rela;"
         xmlns:pstate="&pstate;"
         xmlns:state="&state;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
 	 xmlns:xsd="&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&role;"/>
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&pstate;"/>
     <owl:imports rdf:resource="&state;"/>
  </owl:Ontology>

<!-- Substances -->
  <owl:Class rdf:about="#substanc"/>

  <owl:Class rdf:about="#chemic">
    <rdfs:subClassOf rdf:resource="#substanc"/>
    <owl:equivalentClass rdf:resource="#chemicalsubst"/>
    <rdfs:comment xml:lang="en">Chemical substance, in chemistry, are material objects that can undergo various transformations related to artificial or natural phenomena [Wikipedia]</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#chemicalsubst"/>

  <owl:Class rdf:about="#inorgan">
    <rdfs:subClassOf rdf:resource="#chemicalsubst"/>
    <owl:equivalentClass rdf:resource="#inorganicsubst"/>
  </owl:Class>
  <owl:Class rdf:about="#inorganicsubst"/>

  <owl:Class rdf:about="#medium">
    <rdfs:subClassOf rdf:resource="#substanc"/>
  </owl:Class>

  <owl:Class rdf:about="#mixtur">
    <rdfs:subClassOf rdf:resource="#chemicalsubst"/>
    <owl:equivalentClass rdf:resource="#mixedchemicalsubst"/>
    <rdfs:comment xml:lang="en">In chemistry, a mixture is when two or more different substances are mixed together but not combined chemically. The molecules of two or more different substances are mixed in the form of solutions, suspensions, and colloids. [Wikipedia]</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#mixedchemicalsubst"/>

  <owl:Class rdf:about="#solut">
    <rdfs:subClassOf rdf:resource="#mixtur"/>
    <rdfs:comment xml:lang="en">In chemistry, a solution is a homogeneous mixture composed of two or more substances. In such a mixture, a solute is dissolved in another substance, known as a solvent. All solutions are characterized by interactions between the solvent phase and solute molecules or ions that result in a net decrease in free energy. [Wikipedia]</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#solidsubst">
    <rdfs:subClassOf rdf:resource="#substanc"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat" />
        <owl:hasValue rdf:resource="&pstate;#solid" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#tracesubst">
    <rdfs:subClassOf rdf:resource="#substanc"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasrol" />
        <owl:hasValue rdf:resource="&role;#trace" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#tracega">
    <rdfs:subClassOf rdf:resource="#tracesubst"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat" />
        <owl:hasValue rdf:resource="&pstate;#gas" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#suspendedsubst">
    <rdfs:subClassOf rdf:resource="#substanc"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat" />
        <owl:hasValue rdf:resource="&pstate;#suspend" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#neutralga">
    <rdfs:subClassOf rdf:resource="#chemicalsubst"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat" />
        <owl:hasValue rdf:resource="&pstate;#neutral" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat" />
        <owl:hasValue rdf:resource="&pstate;#gas" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#suspendedga">
    <rdfs:subClassOf rdf:resource="#suspendedsubst"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat" />
        <owl:hasValue rdf:resource="&pstate;#gas" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#suspendedsolid">
    <rdfs:subClassOf rdf:resource="#solidsubst"/>
    <rdfs:subClassOf rdf:resource="#suspendedsubst"/>
  </owl:Class>

  <owl:Class rdf:about="#dissolvedsolid">
    <rdfs:subClassOf rdf:resource="#dissolvedsubst"/>
    <rdfs:subClassOf rdf:resource="#solidsubst"/>
  </owl:Class>

  <owl:Class rdf:about="#air">
    <rdfs:subClassOf rdf:resource="#mixtur"/>
  </owl:Class>

  <owl:Class rdf:about="#greenhousega">
    <rdfs:subClassOf rdf:resource="#chemicalsubst"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat" />
        <owl:hasValue rdf:resource="&pstate;#gas" />
      </owl:Restriction>
    </rdfs:subClassOf>
<!--
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&res;#haseffect" />
        <owl:allValuesFrom rdf:resource="#greenhouseeffect" />
      </owl:Restriction>
    </rdfs:subClassOf>
-->
  </owl:Class>

<!-- Dissolved substanes -->
  <owl:Class rdf:about="#biodegradabledissolvedorganiccarbon">
    <rdfs:subClassOf rdf:resource="#dissolvedorganiccarbon"/>
    <owl:equivalentClass rdf:resource="#bdoc"/>
  </owl:Class>
  <owl:Class rdf:about="#bdoc"/>

  <owl:Class rdf:about="#dissolvedga">
    <rdfs:subClassOf rdf:resource="#dissolvedsubst"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat" />
        <owl:hasValue rdf:resource="&pstate;#gas" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#dissolvedsubst">
    <rdfs:subClassOf rdf:resource="#substanc"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat" />
        <owl:hasValue rdf:resource="&pstate;#dissolv" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#dissolvedoxygen">
    <rdfs:subClassOf rdf:resource="#dissolvedsubst"/>
    <owl:equivalentClass rdf:resource="#do"/>
  </owl:Class>
  <owl:Class rdf:about="#do"/>

  <owl:Class rdf:about="#dissolvedorganiccarbon">
    <rdfs:subClassOf rdf:resource="#dissolvedsubst"/>
    <owl:equivalentClass rdf:resource="#doc"/>
  </owl:Class>
  <owl:Class rdf:about="#doc"/>

  <owl:Class rdf:about="#organicload">
    <rdfs:subClassOf rdf:resource="#dissolvedsubst"/>
  </owl:Class>

</rdf:RDF>
