  <owl:Class rdf:about="#annualcycl">
    <rdfs:subClassOf rdf:resource="#globalcycl"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&trela;#hasfrequ"/>
        <owl:hasValue rdf:resource="&sfreq;#annual"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

</rdf:RDF>
