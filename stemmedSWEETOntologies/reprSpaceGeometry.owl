<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY math "http://sweet.jpl.nasa.gov/2.3/reprMath.owl"> 
  <!ENTITY mathp "http://sweet.jpl.nasa.gov/2.3/reprMathFunction.owl"> 
  <!ENTITY mrela "http://sweet.jpl.nasa.gov/2.3/relaMath.owl"> 
  <!ENTITY srela "http://sweet.jpl.nasa.gov/2.3/relaSpace.owl"> 
  <!ENTITY repr "http://sweet.jpl.nasa.gov/2.3/repr.owl"> 
  <!ENTITY obj "http://sweet.jpl.nasa.gov/2.3/reprSpaceGeometry.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&obj;"
         xmlns:math="&math;"
         xmlns:mathp="&mathp;"
         xmlns:mrela="&mrela;"
         xmlns:srela="&srela;"
         xmlns:repr="&repr;"
         xmlns:obj="&obj;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
	 xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&math;"/>
     <owl:imports rdf:resource="&mathp;"/>
     <owl:imports rdf:resource="&mrela;"/>
     <owl:imports rdf:resource="&srela;"/>
     <owl:imports rdf:resource="&repr;"/>
  </owl:Ontology>

<!-- Classes -->
  <owl:Class rdf:about="#arc">
    <rdfs:subClassOf rdf:resource="#GeometricalObject_1D"/>
  </owl:Class>

  <owl:Class rdf:about="#contour">
    <rdfs:subClassOf rdf:resource="#surfac"/>
  </owl:Class>

  <owl:Class rdf:about="#curv">
    <rdfs:subClassOf rdf:resource="#GeometricalObject_1D"/>
  </owl:Class>

  <owl:Class rdf:about="#geometricalobject">
    <rdfs:subClassOf rdf:resource="&math;#numericalent"/>
  </owl:Class>

  <owl:Class rdf:about="#GeometricalObject_0D">
    <rdfs:subClassOf rdf:resource="#geometricalobject"/>
    <owl:equivalentClass rdf:resource="#point"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="&xsd;integer">0</owl:hasValue>
        <owl:onProperty rdf:resource="&mrela;#hasdimen"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#GeometricalObject_1D">
    <rdfs:subClassOf rdf:resource="#geometricalobject"/>
    <rdfs:subClassOf rdf:resource="&repr;#1d"/>
  </owl:Class>

  <owl:Class rdf:about="#line">
    <rdfs:subClassOf rdf:resource="#curv"/>
  </owl:Class>

  <owl:Class rdf:about="#point">
    <rdfs:subClassOf rdf:resource="#GeometricalObject_0D"/>
  </owl:Class>

  <owl:Class rdf:about="#trajectori">
    <rdfs:subClassOf rdf:resource="#curv"/>
    <owl:equivalentClass rdf:resource="#path"/>
    <rdfs:comment xml:lang="en">Curve in space tracing the points successivley occupied by a particle in motion</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#path"/>

  <owl:Class rdf:about="#rout">
    <rdfs:subClassOf rdf:resource="#path"/>
  </owl:Class>

  <owl:Class rdf:about="#spiral">
    <rdfs:subClassOf rdf:resource="#arc"/>
  </owl:Class>
  
  <owl:Class rdf:about="#boundingbox">
   <rdfs:subClassOf rdf:resource="#rectangl"/>
  </owl:Class>

  <owl:Class rdf:about="#circular">
    <rdfs:subClassOf rdf:resource="#GeometricalObject_2D"/>
  </owl:Class>

  <owl:Class rdf:about="#crescent">
    <rdfs:subClassOf rdf:resource="#GeometricalObject_2D"/>
  </owl:Class>

  <owl:Class rdf:about="#parabol">
    <rdfs:subClassOf rdf:resource="#GeometricalObject_2D"/>
  </owl:Class>

  <owl:Class rdf:about="#circl">
    <rdfs:subClassOf rdf:resource="#conicsect"/>
  </owl:Class>

  <owl:Class rdf:about="#smallcircl">
    <rdfs:subClassOf rdf:resource="#circl"/>
    <owl:disjointWith rdf:resource="#greatcircl"/>
  </owl:Class>

  <owl:Class rdf:about="#greatcircl">
    <rdfs:subClassOf rdf:resource="#circl"/>
  </owl:Class>

  <owl:Class rdf:about="#conic">
    <rdfs:subClassOf rdf:resource="#GeometricalObject_2D"/>
    <owl:equivalentClass rdf:resource="#conicsect"/>
  </owl:Class>
  <owl:Class rdf:about="#conicsect"/>

  <owl:Class rdf:about="#ellip">
    <rdfs:subClassOf rdf:resource="#conicsect"/>
  </owl:Class>

  <owl:Class rdf:about="#GeometricalObject_2D">
    <rdfs:subClassOf rdf:resource="#geometricalobject"/>
    <rdfs:subClassOf rdf:resource="&repr;#2d"/>
  </owl:Class>

  <owl:Class rdf:about="#helic">
    <rdfs:subClassOf rdf:resource="#GeometricalObject_2D"/>
  </owl:Class>

  <owl:Class rdf:about="#hyperbola">
    <rdfs:subClassOf rdf:resource="#conicsect"/>
  </owl:Class>

  <owl:Class rdf:about="#hyperplan">
    <rdfs:subClassOf rdf:resource="#geometricalobject"/>
  </owl:Class>

  <owl:Class rdf:about="#parabola">
    <rdfs:subClassOf rdf:resource="#conicsect"/>
  </owl:Class>

  <owl:Class rdf:about="#plane">
    <rdfs:subClassOf rdf:resource="#surfac"/>
    <rdfs:subClassOf rdf:resource="#GeometricalObject_2D"/>
  </owl:Class>

  <owl:Class rdf:about="#polygon">
    <rdfs:subClassOf rdf:resource="#GeometricalObject_2D"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#point"/>
        <owl:onProperty rdf:resource="&srela;#hasvert"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#rectangl">
    <rdfs:subClassOf rdf:resource="#polygon"/>
  </owl:Class>

  <owl:Class rdf:about="#squar">
    <rdfs:subClassOf rdf:resource="#rectangl"/>
  </owl:Class>

  <owl:Class rdf:about="#star">
    <rdfs:subClassOf rdf:resource="#GeometricalObject_2D"/>
  </owl:Class>

  <owl:Class rdf:about="#surfac">
    <rdfs:subClassOf rdf:resource="#geometricalobject"/>
  </owl:Class>

  <owl:Class rdf:about="#patch">
    <rdfs:subClassOf rdf:resource="#surfac"/>
  </owl:Class>

  <owl:Class rdf:about="#lattic">
    <rdfs:subClassOf rdf:resource="#geometricalobject"/>
    <rdfs:subClassOf rdf:resource="&mathp;#discret"/>
  </owl:Class>

  <owl:Class rdf:about="#region">
    <rdfs:subClassOf rdf:resource="#geometricalobject"/>
  </owl:Class>

  <owl:Class rdf:about="#cluster">
    <rdfs:subClassOf rdf:resource="&math;#collect"/>
    <owl:equivalentClass rdf:resource="#famili"/>
    <owl:equivalentClass rdf:resource="#swarm"/>
  </owl:Class>
  <owl:Class rdf:about="#famili"/>
  <owl:Class rdf:about="#swarm"/>

  <owl:Class rdf:about="#zone">
    <rdfs:subClassOf rdf:resource="#region"/>
  </owl:Class>

  <owl:Class rdf:about="#ridg">
    <rdfs:subClassOf rdf:resource="#geometricalobject"/>
  </owl:Class>

  <owl:Class rdf:about="#depress">
    <rdfs:subClassOf rdf:resource="#geometricalobject"/>
  </owl:Class>

  <owl:Class rdf:about="#trough">
    <rdfs:subClassOf rdf:resource="#geometricalobject"/>
  </owl:Class>

  <owl:Class rdf:about="#peak">
    <rdfs:subClassOf rdf:resource="#geometricalobject"/>
  </owl:Class>

</rdf:RDF>
