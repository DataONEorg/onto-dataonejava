<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [

  <!ENTITY root "http://sweet.jpl.nasa.gov/2.3/">
  <!ENTITY trela "&root;relaTime.owl#">
  <!ENTITY day "&root;reprTimeDay.owl#">
  <!ENTITY owlt "http://www.w3.org/2006/time#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&day;"
         xmlns:trela="&trela;"
         xmlns:day="&day;"
         xmlns:owlt="&owlt;"
         xmlns:xsd="&xsd;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&root;relaTime.owl"/>
  </owl:Ontology>

<!-- Diurnal definitions -->
  <owl:Class rdf:about="#noon">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
  </owl:Class>

  <owl:Class rdf:about="#day">
    <rdfs:subClassOf rdf:resource="&owlt;ProperInterval"/>
  </owl:Class>

  <owl:Class rdf:about="#daytim">
    <rdfs:subClassOf rdf:resource="&owlt;ProperInterval"/>
  </owl:Class>

  <owl:Class rdf:about="#midnight">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
  </owl:Class>

  <owl:Class rdf:about="#nighttim">
    <rdfs:subClassOf rdf:resource="&owlt;ProperInterval"/>
    <owl:equivalentClass rdf:resource="#nocturn"/>
  </owl:Class>
  <owl:Class rdf:about="#nocturn"/>

  <owl:Class rdf:about="#evene">
    <rdfs:subClassOf rdf:resource="#nighttim"/>
  </owl:Class>

  <owl:Class rdf:about="#morn">
    <rdfs:subClassOf rdf:resource="#daytim"/>
  </owl:Class>

  <owl:Class rdf:about="#sunset">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#evene"/>
        <owl:onProperty rdf:resource="&trela;startOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#sunri">
    <rdfs:subClassOf rdf:resource="&owlt;Instant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#morn"/>
        <owl:onProperty rdf:resource="&trela;startOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#dawn">
    <rdfs:subClassOf rdf:resource="#twilight"/>
  </owl:Class>

  <owl:Class rdf:about="#dusk">
    <rdfs:subClassOf rdf:resource="#twilight"/>
  </owl:Class>

  <owl:Class rdf:about="#twilight">
    <rdfs:subClassOf rdf:resource="&owlt;ProperInterval"/>
  </owl:Class>

</rdf:RDF>
