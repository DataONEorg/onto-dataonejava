<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY hum "http://sweet.jpl.nasa.gov/2.3/human.owl">
  <!ENTITY trans "http://sweet.jpl.nasa.gov/2.3/humanTransportation.owl">
  <!ENTITY trela "http://sweet.jpl.nasa.gov/2.3/relaTime.owl">
  <!ENTITY hrela "http://sweet.jpl.nasa.gov/2.3/relaHuman.owl">
  <!ENTITY comm "http://sweet.jpl.nasa.gov/2.3/humanCommerce.owl">
  <!ENTITY equi "http://sweet.jpl.nasa.gov/2.3/matrEquipment.owl">
  <!ENTITY jur "http://sweet.jpl.nasa.gov/2.3/humanJurisdiction.owl">
  <!ENTITY jur2 "http://sweet.jpl.nasa.gov/2.3/humanJurisdiction.owl#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&trans;"
         xmlns:hum="&hum;"
         xmlns:trans="&trans;"
         xmlns:trela="&trela;"
         xmlns:hrela="&hrela;"
         xmlns:comm="&comm;"
         xmlns:equi="&equi;"
         xmlns:jur="&jur;"
         xmlns:jur2="&jur2;"
         xmlns:owl="&owl;"
         xmlns:xsd="&xsd;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&hum;"/>   
     <owl:imports rdf:resource="&comm;"/>   
     <owl:imports rdf:resource="&hrela;"/>   
     <owl:imports rdf:resource="&trela;"/>   
     <owl:imports rdf:resource="&jur;"/>
     <owl:imports rdf:resource="&equi;"/>   
  </owl:Ontology>

  <owl:Class rdf:about="#evacuat">
    <rdfs:subClassOf rdf:resource="&hum;#humanact"/>
  </owl:Class>

  <owl:Class rdf:about="#transit">
    <rdfs:subClassOf rdf:resource="#transport"/>
  </owl:Class>

  <owl:Class rdf:about="#transport">
    <rdfs:subClassOf rdf:resource="&hum;#humanact"/>
  </owl:Class>

  <owl:Class rdf:about="#transport">
    <rdfs:subClassOf rdf:resource="&hum;#humanact"/>
  </owl:Class>

  <owl:Class rdf:about="#groundtransport">
    <rdfs:subClassOf rdf:resource="#transport"/>
  </owl:Class>

  <owl:Class rdf:about="#airtransport">
    <rdfs:subClassOf rdf:resource="#transport"/>
  </owl:Class>

  <owl:Class rdf:about="#arrival">
    <rdfs:subClassOf rdf:resource="&hum;#humanact"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&trela;#hasendtim" />
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#departur">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&trela;#hasstarttim" />
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="&hum;#humanact"/>
  </owl:Class>

<!-- Human activities / phenomena -->
  <owl:Class rdf:about="#aviation">
    <rdfs:subClassOf rdf:resource="&hum;#humanact" />
  </owl:Class>

  <owl:Class rdf:about="#civilavi">
    <rdfs:subClassOf rdf:resource="#aviation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&hrela;#hasjurisdict" />
        <owl:allValuesFrom rdf:resource="&jur;#civil" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#militaryavi">
    <rdfs:subClassOf rdf:resource="#aviation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&hrela;#hasjurisdict" />
        <owl:allValuesFrom rdf:resource="&jur;#militari" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#airtraff">
    <rdfs:subClassOf rdf:resource="&hum;#humanact"/>
  </owl:Class>

  <owl:Class rdf:about="#airtrafficcontrol">
    <rdfs:subClassOf rdf:resource="&comm;#manag"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&hrela;#hashumanact" />
        <owl:allValuesFrom rdf:resource="#airtraff" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#flight">
    <rdfs:subClassOf rdf:resource="#airtransport"/>
  </owl:Class>

  <owl:Class rdf:about="#land">
    <rdfs:subClassOf rdf:resource="#arrival"/>
  </owl:Class>

  <owl:Class rdf:about="#takeoff">
    <rdfs:subClassOf rdf:resource="#departur"/>
  </owl:Class>

  <owl:Class rdf:about="#liftoff">
    <rdfs:subClassOf rdf:resource="#departur"/>
  </owl:Class>

  <owl:Class rdf:about="#ascent">
    <rdfs:subClassOf rdf:resource="#departur"/>
  </owl:Class>

  <owl:Class rdf:about="#descent">
    <rdfs:subClassOf rdf:resource="#arrival"/>
  </owl:Class>

  <owl:Class rdf:about="#spaceprogram">
    <rdfs:subClassOf rdf:resource="&hum;#humanact"/>
  </owl:Class>

  <owl:Class rdf:about="#spacetravel">
    <rdfs:subClassOf rdf:resource="#spaceprogram"/>
  </owl:Class>

  <jur2:FederalGoverningBody rdf:about="#faa">
    <owl:sameAs rdf:resource="#federalaviationadministr" />
  </jur2:FederalGoverningBody>
  <jur2:FederalGoverningBody rdf:about="#federalaviationadministr"/>

</rdf:RDF>
