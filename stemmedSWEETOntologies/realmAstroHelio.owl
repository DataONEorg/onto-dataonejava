<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY planet "http://sweet.jpl.nasa.gov/2.3/realm.owl">
  <!ENTITY helio "http://sweet.jpl.nasa.gov/2.3/realmAstroHelio.owl">
  <!ENTITY srela "http://sweet.jpl.nasa.gov/2.3/relaSpace.owl">
  <!ENTITY prela "http://sweet.jpl.nasa.gov/2.3/relaPhysical.owl">
  <!ENTITY rela "http://sweet.jpl.nasa.gov/2.3/rela.owl">
  <!ENTITY band "http://sweet.jpl.nasa.gov/2.3/stateSpectralBand.owl">
  <!ENTITY ordi "http://sweet.jpl.nasa.gov/2.3/propOrdinal.owl">
  <!ENTITY state "http://sweet.jpl.nasa.gov/2.3/state.owl">
  <!ENTITY pstate "http://sweet.jpl.nasa.gov/2.3/statePhysical.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&helio;"
	 xmlns:planet="&planet;"
	 xmlns:helio="&helio;"
	 xmlns:srela="&srela;"
	 xmlns:prela="&prela;"
	 xmlns:rela="&rela;"
	 xmlns:band="&band;"
	 xmlns:ordi="&ordi;"
	 xmlns:state="&state;"
	 xmlns:pstate="&pstate;"
	 xmlns:owl="&owl;"
	 xmlns:rdf="&rdf;"
	 xmlns:rdfs="&rdfs;"
	 xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
		owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&planet;"/>
     <owl:imports rdf:resource="&srela;"/>
     <owl:imports rdf:resource="&prela;"/>
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&state;"/>
     <owl:imports rdf:resource="&pstate;"/>
     <owl:imports rdf:resource="&band;"/>
     <owl:imports rdf:resource="&ordi;"/>
  </owl:Ontology>

  <owl:AnnotationProperty rdf:about="&owl;versionInfo"/>

<!-- Realms -->
  <owl:Class rdf:ID="UpperHeliosphere">
    <rdfs:subClassOf rdf:resource="&planet;#planetarylay" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasrealm"/>
        <owl:allValuesFrom rdf:resource="#heliosph"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#heliosph">
    <rdfs:subClassOf rdf:resource="&planet;#planetarylay"/>
  </owl:Class>

  <owl:Class rdf:about="#exospher">
    <rdfs:subClassOf rdf:resource="&planet;#planetarylay"/>
  </owl:Class>

  <owl:Class rdf:about="#magnetosph">
    <rdfs:subClassOf rdf:resource="&planet;#planetarylay"/>
  </owl:Class>

  <owl:Class rdf:about="#ionosph">
    <rdfs:subClassOf rdf:resource="&planet;#planetarylay"/>
  </owl:Class>

  <owl:Class rdf:ID="DLayer">
    <rdfs:subClassOf rdf:resource="&planet;#planetarylay" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasrealm"/>
        <owl:someValuesFrom rdf:resource="#ionosph"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasspectralband"/>
        <owl:hasValue rdf:resource="&band;#hardxray"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="ELayer">
    <rdfs:subClassOf rdf:resource="&planet;#planetarylay" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasrealm"/>
        <owl:someValuesFrom rdf:resource="#ionosph"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasspectralband"/>
        <owl:hasValue rdf:resource="&band;#softxray"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="FLayer">
    <rdfs:subClassOf rdf:resource="&planet;#planetarylay" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasrealm"/>
        <owl:someValuesFrom rdf:resource="#ionosph"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasspectralband"/>
        <owl:hasValue rdf:resource="&band;#euv"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="LowerHeliosphere">
    <rdfs:subClassOf rdf:resource="&planet;#planetarylay" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasrealm"/>
        <owl:someValuesFrom rdf:resource="#heliosph"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#plasmasph">
    <rdfs:subClassOf rdf:resource="&planet;#planetarylay" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasrealm"/>
        <owl:someValuesFrom rdf:resource="#lowerheliosph"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat"/>
        <owl:hasValue rdf:resource="&pstate;#plasma"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#hasupperboundari"/>
        <owl:allValuesFrom rdf:resource="#plasmapaus"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#radiationbelt">
    <owl:equivalentClass rdf:resource="#vanallenbelt"/>
    <rdfs:subClassOf rdf:resource="&planet;#planetarylay" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasrealm"/>
        <owl:allValuesFrom rdf:resource="#heliosph"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#vanallenbelt"/>

  <owl:Class rdf:about="#plasmapaus">
    <rdfs:subClassOf rdf:resource="&planet;#planetaryboundari" />
  </owl:Class>

  <owl:Class rdf:about="#heliopaus">
    <rdfs:subClassOf rdf:resource="&planet;#planetarysurfacelevel"/>
  </owl:Class>

</rdf:RDF>
