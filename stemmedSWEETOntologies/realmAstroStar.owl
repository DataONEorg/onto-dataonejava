<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY astro "http://sweet.jpl.nasa.gov/2.3/realmAstroBody.owl">
  <!ENTITY realm "http://sweet.jpl.nasa.gov/2.3/realm.owl">
  <!ENTITY rela "http://sweet.jpl.nasa.gov/2.3/rela.owl">
  <!ENTITY crela "http://sweet.jpl.nasa.gov/2.3/relaChemical.owl">
  <!ENTITY state "http://sweet.jpl.nasa.gov/2.3/state.owl">
  <!ENTITY pstate "http://sweet.jpl.nasa.gov/2.3/statePhysical.owl">
  <!ENTITY cproc "http://sweet.jpl.nasa.gov/2.3/procChemical.owl">
  <!ENTITY star "http://sweet.jpl.nasa.gov/2.3/realmAstroStar.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&star;"
         xmlns:owl="&owl;"
         xmlns:astro="&astro;"
         xmlns:realm="&realm;"
         xmlns:cproc="&cproc;"
         xmlns:state="&state;"
         xmlns:pstate="&pstate;"
         xmlns:star="&star;"
         xmlns:crela="&crela;"
         xmlns:rela="&rela;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
	 xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&realm;"/>
     <owl:imports rdf:resource="&crela;"/>
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&cproc;"/>
     <owl:imports rdf:resource="&state;"/>
     <owl:imports rdf:resource="&pstate;"/>
     <owl:imports rdf:resource="&astro;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#activeregion">
    <rdfs:subClassOf rdf:resource="#stellarrealm"/>
  </owl:Class>

  <owl:Class rdf:about="#chromosph">
    <rdfs:subClassOf rdf:resource="#stellaratmosph"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat"/>
        <owl:hasValue rdf:resource="&pstate;#plasma"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#corona">
    <rdfs:subClassOf rdf:resource="#stellaratmosph"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasstat"/>
        <owl:hasValue rdf:resource="&pstate;#plasma"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#photosph">
    <rdfs:subClassOf rdf:resource="#stellaratmosph"/>
  </owl:Class>

  <owl:Class rdf:about="#stellaratmosph">
    <rdfs:subClassOf rdf:resource="#stellarrealm"/>
  </owl:Class>

  <owl:Class rdf:about="#stellarinterior">
    <rdfs:subClassOf rdf:resource="#stellarrealm"/>
  </owl:Class>

  <owl:Class rdf:about="#convectionzon">
    <rdfs:subClassOf rdf:resource="#stellarinterior"/>
    <owl:disjointWith rdf:resource="#stellarcor" />
  </owl:Class>

  <owl:Class rdf:about="#radiativezon">
    <rdfs:subClassOf rdf:resource="#stellarinterior"/>
  </owl:Class>

  <owl:Class rdf:about="#stellarcor">
    <rdfs:subClassOf rdf:resource="#stellarinterior"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&crela;#haschemicalreact"/>
        <owl:someValuesFrom rdf:resource="&cproc;#nuclearfus"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#stellarrealm">
    <rdfs:subClassOf rdf:resource="&realm;#realm"/>
  </owl:Class>

  <owl:Class rdf:about="#tacoclin">
    <rdfs:subClassOf rdf:resource="#stellarinterior"/>
  </owl:Class>

</rdf:RDF>
