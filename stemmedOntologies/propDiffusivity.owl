<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY func "http://sweet.jpl.nasa.gov/2.3/reprMathFunction.owl">
  <!ENTITY rate "http://sweet.jpl.nasa.gov/2.3/propTimeFrequency.owl">
  <!ENTITY diff "http://sweet.jpl.nasa.gov/2.3/propDiffusivity.owl">
  <!ENTITY proc "http://sweet.jpl.nasa.gov/2.3/procPhysical.owl">
  <!ENTITY quan "http://sweet.jpl.nasa.gov/2.3/propQuantity.owl">
  <!ENTITY wave "http://sweet.jpl.nasa.gov/2.3/phenFluidDynamics.owl">
  <!ENTITY units "http://sweet.jpl.nasa.gov/2.3/reprSciUnits.owl">
  <!ENTITY screla "http://sweet.jpl.nasa.gov/2.3/relaSci.owl">
  <!ENTITY rela "http://sweet.jpl.nasa.gov/2.3/rela.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&diff;"
         xmlns:units="&units;"
         xmlns:diff="&diff;"
         xmlns:rela="&rela;"
         xmlns:screla="&screla;"
         xmlns:wave="&wave;"
         xmlns:quan="&quan;"
         xmlns:proc="&proc;"
         xmlns:rate="&rate;"
         xmlns:func="&func;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
 	 xmlns:xsd="&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&quan;"/>
     <owl:imports rdf:resource="&units;"/>
     <owl:imports rdf:resource="&func;"/>
     <owl:imports rdf:resource="&wave;"/>
     <owl:imports rdf:resource="&proc;"/>
     <owl:imports rdf:resource="&rate;"/>
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&screla;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#Transmiss">
    <rdfs:subClassOf rdf:resource="#DiffusivityQuant"/>
  </owl:Class>

  <owl:Class rdf:about="#DiffusivityQuant">
    <rdfs:subClassOf rdf:resource="&quan;#PhysicalQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#meterSquaredPerSecond"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Diffus">
    <rdfs:subClassOf rdf:resource="#DiffusivityQuant"/>
  </owl:Class>

  <owl:Class rdf:about="#EddyDiffus">
    <rdfs:subClassOf rdf:resource="#Diffus" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasPhenomena" />
        <owl:someValuesFrom rdf:resource="&wave;#Eddi" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">The exchange coefficient for the diffusion of a conservative property by eddies in a turbulent flow.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#KinematicViscos">
    <rdfs:subClassOf rdf:resource="#DiffusivityQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="&proc;#ViscousForc"/>
        <owl:onProperty rdf:resource="&screla;#measureOf"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ThermalDiffus">
    <rdfs:subClassOf rdf:resource="#Diffus" />
    <rdfs:subClassOf rdf:resource="&quan;#ThermodynamicQuant"/>
  </owl:Class>

  <owl:Class rdf:about="#MeridionalOvertuningStreamfunct">
    <rdfs:subClassOf rdf:resource="#Streamfunct"/>
  </owl:Class>

  <owl:Class rdf:about="#Streamfunct">
    <rdfs:subClassOf rdf:resource="&func;#VectorQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#meterCubedPerSecond"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#VelocityPotenti">
    <rdfs:subClassOf rdf:resource="#DiffusivityQuant"/>
    <rdfs:subClassOf rdf:resource="&func;#VectorQuant"/>
  </owl:Class>

  <owl:Class rdf:about="#Diffluenc">
    <rdfs:subClassOf rdf:resource="#DiffusivityQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasPhenomena" />
        <owl:someValuesFrom rdf:resource="&rate;#Diverg" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">The rate at which adjacent flow diverges along an axis oriented normal to the flow at the point in question; the opposite of confluence.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Confluenc">
    <rdfs:subClassOf rdf:resource="#DiffusivityQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasPhenomena" />
        <owl:someValuesFrom rdf:resource="&rate;#Converg" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">The rate at which adjacent flow is converging along an axis oriented normal to the flow at the point in question.</rdfs:comment>
  </owl:Class>

</rdf:RDF>
