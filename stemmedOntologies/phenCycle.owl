<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY planet "http://sweet.jpl.nasa.gov/2.3/realm.owl">
  <!ENTITY rela "http://sweet.jpl.nasa.gov/2.3/rela.owl">
  <!ENTITY prela "http://sweet.jpl.nasa.gov/2.3/relaPhysical.owl">
  <!ENTITY srela "http://sweet.jpl.nasa.gov/2.3/relaSpace.owl">
  <!ENTITY trela "http://sweet.jpl.nasa.gov/2.3/relaTime.owl">
  <!ENTITY osc "http://sweet.jpl.nasa.gov/2.3/phenCycle.owl"> 
  <!ENTITY sys "http://sweet.jpl.nasa.gov/2.3/phenSystem.owl"> 
  <!ENTITY spac "http://sweet.jpl.nasa.gov/2.3/stateSpaceScale.owl">
  <!ENTITY star "http://sweet.jpl.nasa.gov/2.3/phenStar.owl">
  <!ENTITY sfreq "http://sweet.jpl.nasa.gov/2.3/stateTimeFrequency.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">

]>

<rdf:RDF xml:base="&osc;"
         xmlns:planet="&planet;"
         xmlns:sfreq="&sfreq;"
         xmlns:spac="&spac;"
         xmlns:star="&star;"
         xmlns:rela="&rela;"
         xmlns:trela="&trela;"
         xmlns:srela="&srela;"
         xmlns:prela="&prela;"
         xmlns:osc="&osc;"
         xmlns:sys="&sys;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
	 xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&spac;"/>
     <owl:imports rdf:resource="&sys;"/>
     <owl:imports rdf:resource="&sfreq;"/>     
     <owl:imports rdf:resource="&star;"/>     
     <owl:imports rdf:resource="&rela;"/>     
     <owl:imports rdf:resource="&trela;"/>     
     <owl:imports rdf:resource="&srela;"/>     
     <owl:imports rdf:resource="&prela;"/>     
     <owl:imports rdf:resource="&planet;"/>
  </owl:Ontology>

<!-- Phenomena -->
  <owl:Class rdf:about="#EarthOscil">
    <rdfs:subClassOf rdf:resource="#PlanetaryOscil"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&prela;#hasAstronomicalBodi" />
        <owl:hasValue rdf:resource="&planet;#Earth" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#AntarcticOscil">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
  </owl:Class>

  <owl:Class rdf:about="#ArcticOscil">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
  </owl:Class>

  <owl:Class rdf:about="#ChandlerWobbl">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
  </owl:Class>


  <owl:Class rdf:about="#MaddenJulianOscil">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&trela;#hasFrequ"/>
        <owl:hasValue rdf:resource="&sfreq;#Decad"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#MilankovitchCycl">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
  </owl:Class>

  <owl:Class rdf:about="#NorthAtlanticOscil">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
    <owl:equivalentClass rdf:resource="#NAO"/>
  </owl:Class>
  <owl:Class rdf:about="#NAO"/>

  <owl:Class rdf:about="#PacificDecadalOscil">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
    <owl:equivalentClass rdf:resource="#PDO"/>
  </owl:Class>
  <owl:Class rdf:about="#PDO"/>

  <owl:Class rdf:about="#PacificNorthAmericanPattern">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
  </owl:Class>

  <owl:Class rdf:about="#QuasiBiennialOscil">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&trela;#hasFrequ"/>
        <owl:hasValue rdf:resource="&sfreq;#Biannual"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass rdf:resource="#QBO"/>
  </owl:Class>
  <owl:Class rdf:about="#QBO"/>

  <owl:Class rdf:about="#SouthernOscil">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
    <owl:equivalentClass rdf:resource="#ENSO"/>
  </owl:Class>

  <owl:Class rdf:about="#PlanetaryOscil">
    <rdfs:subClassOf rdf:resource="#GlobalCycl"/>
  </owl:Class>

  <owl:Class rdf:about="#GlobalCycl">
    <rdfs:subClassOf rdf:resource="&sys;#Oscillat"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&srela;#hasSpatialScal" />
        <owl:hasValue rdf:resource="&spac;#Global" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#PolarWand">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
  </owl:Class>

  <owl:Class rdf:about="#BodyTid">
    <rdfs:subClassOf rdf:resource="#Tide"/>
  </owl:Class>

  <owl:Class rdf:about="#PoleTid">
    <rdfs:subClassOf rdf:resource="#Tide"/>
  </owl:Class>

  <owl:Class rdf:about="#Tide">
    <rdfs:subClassOf rdf:resource="#PlanetaryOscil"/>
  </owl:Class>

  <owl:Class rdf:about="#IceAgeCycl">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
  </owl:Class>

  <owl:Class rdf:about="#ElNinoSouthernOscil">
    <rdfs:subClassOf rdf:resource="#EarthOscil"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasRealm" />
        <owl:allValuesFrom rdf:resource="&planet;#Ocean" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass rdf:resource="#ENSO"/>
    <rdfs:comment xml:lang="en">A significant increase in sea surface temperature over the eastern and central equatorial Pacific that occurs at irregular intervals, generally ranging between two and seven years.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#ENSO"/>

  <owl:Class rdf:about="#SolarCycl">
    <rdfs:subClassOf rdf:resource="#GlobalCycl"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasPhenomena" />
        <owl:allValuesFrom rdf:resource="&star;#SolarAct" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#DiurnalCycl">
    <rdfs:subClassOf rdf:resource="#GlobalCycl"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&trela;#hasFrequ"/>
        <owl:hasValue rdf:resource="&sfreq;#Daili"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#AnnualCycl">
    <rdfs:subClassOf rdf:resource="#GlobalCycl"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&trela;#hasFrequ"/>
        <owl:hasValue rdf:resource="&sfreq;#Annual"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

</rdf:RDF>
