<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY reac "http://sweet.jpl.nasa.gov/2.3/phenReaction.owl">
  <!ENTITY rela "http://sweet.jpl.nasa.gov/2.3/rela.owl">
  <!ENTITY screla "http://sweet.jpl.nasa.gov/2.3/relaSci.owl">
  <!ENTITY crela "http://sweet.jpl.nasa.gov/2.3/relaChemical.owl">
  <!ENTITY isot "http://sweet.jpl.nasa.gov/2.3/matrIsotope.owl">
  <!ENTITY cproc "http://sweet.jpl.nasa.gov/2.3/procChemical.owl">
  <!ENTITY noise "http://sweet.jpl.nasa.gov/2.3/phenWaveNoise.owl">
  <!ENTITY role "http://sweet.jpl.nasa.gov/2.3/stateRole.owl">
  <!ENTITY impa "http://sweet.jpl.nasa.gov/2.3/stateRoleImpact.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&reac;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:reac="&reac;"
         xmlns:isot="&isot;"
         xmlns:role="&role;"
         xmlns:rela="&rela;"
         xmlns:screla="&screla;"
         xmlns:crela="&crela;"
         xmlns:impa="&impa;"
         xmlns:cproc="&cproc;"
         xmlns:noise="&noise;"
>

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&cproc;"/>
     <owl:imports rdf:resource="&noise;"/>
     <owl:imports rdf:resource="&role;"/>
     <owl:imports rdf:resource="&impa;"/>
     <owl:imports rdf:resource="&isot;"/>
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&screla;"/>
     <owl:imports rdf:resource="&crela;"/>
  </owl:Ontology>

<!-- Phenomena -->
  <owl:Class rdf:about="#Explosion">
    <rdfs:subClassOf rdf:resource="&cproc;#ChemicalReact"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&screla;#hasImpact" />
        <owl:hasValue rdf:resource="&impa;#Hazard" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasPhenomena" />
        <owl:allValuesFrom rdf:resource="&noise;#Nois" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Fire">
    <rdfs:subClassOf rdf:resource="&cproc;#ChemicalReact"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&screla;#hasImpact" />
        <owl:hasValue rdf:resource="&impa;#Hazard" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Flare">
    <rdfs:subClassOf rdf:resource="&cproc;#ChemicalReact"/>
    <owl:equivalentClass rdf:resource="#Flare"/>
  </owl:Class>
  <owl:Class rdf:about="#Flare"/>

  <owl:Class rdf:about="#FluidLeak">
    <rdfs:subClassOf rdf:resource="#Leak"/>
  </owl:Class>

  <owl:Class rdf:about="#GasFlar">
    <rdfs:subClassOf rdf:resource="&cproc;#ChemicalReact"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&screla;#hasImpact" />
        <owl:hasValue rdf:resource="&impa;#Hazard" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#GasLeak">
    <rdfs:subClassOf rdf:resource="#Leak"/>
  </owl:Class>

  <owl:Class rdf:about="#Emission">
    <rdfs:subClassOf rdf:resource="&cproc;#ChemicalReact"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasRol" />
        <owl:hasValue rdf:resource="&role;#ByProduct" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Eruption">
    <rdfs:subClassOf rdf:resource="#Releas"/>
  </owl:Class>

  <owl:Class rdf:about="#Activat">
    <rdfs:subClassOf rdf:resource="&cproc;#ChemicalReact"/>
  </owl:Class>

  <owl:Class rdf:about="#IndustrialEmiss">
    <rdfs:subClassOf rdf:resource="#Emission"/>
  </owl:Class>

  <owl:Class rdf:about="#Leak">
    <rdfs:subClassOf rdf:resource="#Releas"/>
  </owl:Class>

  <owl:Class rdf:about="#Leach">
    <rdfs:subClassOf rdf:resource="#Releas"/>
  </owl:Class>

  <owl:Class rdf:about="#Releas">
    <rdfs:subClassOf rdf:resource="#Emission"/>
  </owl:Class>

  <owl:Class rdf:about="#BiochemicalReleas">
    <rdfs:subClassOf rdf:resource="#Releas"/>
  </owl:Class>

  <owl:Class rdf:about="#RadioactivityReleas">
    <rdfs:subClassOf rdf:resource="#Releas"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&crela;#hasSubst" />
        <owl:allValuesFrom rdf:resource="&isot;#RadioactiveSubst" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Wildfir">
    <rdfs:subClassOf rdf:resource="#Fire"/>
  </owl:Class>

</rdf:RDF>
