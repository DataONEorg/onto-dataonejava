<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
   <!ENTITY serv "http://sweet.jpl.nasa.gov/2.3/reprDataService.owl#">   
   <!ENTITY solu "http://sweet.jpl.nasa.gov/2.3/reprMathSolution.owl">   
   <!ENTITY huma "http://sweet.jpl.nasa.gov/2.3/human.owl">   
   <!ENTITY prrela "http://sweet.jpl.nasa.gov/2.3/relaProvenance.owl">   
   <!ENTITY owl "http://www.w3.org/2002/07/owl#">
   <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
   <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
   <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF
    xmlns = "&serv;"
    xml:base = "&serv;"
    xmlns:serv = "&serv;"
    xmlns:solu = "&solu;"
    xmlns:huma = "&huma;"
    xmlns:prrela = "&prrela;"
    xmlns:owl = "&owl;"
    xmlns:rdf = "&rdf;"
    xmlns:rdfs = "&rdfs;"
    xmlns:xsd = "&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
    <owl:imports rdf:resource = "&huma;"/>
    <owl:imports rdf:resource = "&prrela;"/>
    <owl:imports rdf:resource = "&solu;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#DataServic">
    <rdfs:subClassOf rdf:resource="&huma;#Servic" />
  </owl:Class>

  <owl:Class rdf:about="#PostProcess">
    <rdfs:subClassOf rdf:resource="#DataServic"/>
  </owl:Class>

  <owl:Class rdf:about="#PreProcess">
    <rdfs:subClassOf rdf:resource="#DataServic"/>
  </owl:Class>

  <owl:Class rdf:about="#DataSynchron">
    <rdfs:subClassOf rdf:resource="#DataServic"/>
  </owl:Class>

  <owl:Class rdf:about="#Discov">
    <rdfs:subClassOf rdf:resource="#DataServic"/>
    <owl:equivalentClass rdf:resource="#Discoveri"/>
    <owl:equivalentClass rdf:resource="#DiscoveryServic"/>
  </owl:Class>
  <owl:Class rdf:about="#Discoveri"/>
  <owl:Class rdf:about="#DiscoveryServic"/>

<!-- Data Format -->
  <owl:Class rdf:about="#Reformat">
    <rdfs:subClassOf rdf:resource="#DataServic"/>
    <owl:equivalentClass rdf:resource="#FormatConvers"/>
    <owl:equivalentClass rdf:resource="#Reformat"/>
  </owl:Class>
  <owl:Class rdf:about="#FormatConvers"/>
  <owl:Class rdf:about="#Reformat"/>

<!-- Compression -->
  <owl:Class rdf:about="#Compress">
    <rdfs:subClassOf rdf:resource="#DataServic"/>
  </owl:Class>

  <owl:Class rdf:about="#CompressionMethod">
    <rdfs:subClassOf rdf:resource="&solu;#Algorithm"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Compress"/>
        <owl:onProperty rdf:resource="&prrela;#hasDataServic"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <serv:CompressionMethod rdf:about="#Bzip2">
  </serv:CompressionMethod>

  <serv:CompressionMethod rdf:about="#Gzip">
  </serv:CompressionMethod>

  <serv:CompressionMethod rdf:about="#Z">
  </serv:CompressionMethod>

<!-- Data Access -->
  <owl:Class rdf:about="#Access">
    <rdfs:subClassOf rdf:resource="#DataServic"/>
    <owl:equivalentClass rdf:resource="#Access"/>
  </owl:Class>
  <owl:Class rdf:about="#Access"/>

  <owl:Class rdf:about="#InterfaceProtocol">
    <rdfs:subClassOf rdf:resource="#DataServic"/>
  </owl:Class>

  <serv:InterfaceProtocol rdf:about="#OPeNDAP">
    <owl:sameAs rdf:resource="#DODS"/>
  </serv:InterfaceProtocol>
  <serv:InterfaceProtocol rdf:about="#DODS"/>

</rdf:RDF>
