<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY agri "http://sweet.jpl.nasa.gov/2.3/humanAgriculture.owl">
  <!ENTITY comm "http://sweet.jpl.nasa.gov/2.3/humanCommerce.owl">
  <!ENTITY crela "http://sweet.jpl.nasa.gov/2.3/relaChemical.owl">
  <!ENTITY screla "http://sweet.jpl.nasa.gov/2.3/relaSci.owl">
  <!ENTITY comp "http://sweet.jpl.nasa.gov/2.3/matrCompound.owl">
  <!ENTITY anim "http://sweet.jpl.nasa.gov/2.3/matrAnimal.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&agri;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
         xmlns:xsd="&xsd;"
         xmlns:comm="&comm;"
         xmlns:agri="&agri;"
         xmlns:crela="&crela;"
         xmlns:screla="&screla;"
         xmlns:comp="&comp;"
         xmlns:anim="&anim;"
>
<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about=""
                owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&comp;"/>
     <owl:imports rdf:resource="&comm;"/>
     <owl:imports rdf:resource="&anim;"/>
     <owl:imports rdf:resource="&screla;"/>
     <owl:imports rdf:resource="&crela;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#Agricultur">
    <rdfs:subClassOf rdf:resource="&comm;#Product"/>
    <rdfs:subClassOf rdf:resource="&comm;#LandUs"/>
  </owl:Class>

  <owl:Class rdf:about="#CropProduct">
    <rdfs:subClassOf rdf:resource="#PlantProduct"/>
  </owl:Class>

  <owl:Class rdf:about="#Cultiv"> 
    <rdfs:subClassOf rdf:resource="#Agricultur"/>
  </owl:Class>

  <owl:Class rdf:about="#DripIrrig">
    <rdfs:subClassOf rdf:resource="#Agricultur"/>
  </owl:Class>

  <owl:Class rdf:about="#Log">
    <rdfs:subClassOf rdf:resource="&comm;#Product"/>
  </owl:Class>

  <owl:Class rdf:about="#Harvest">
    <rdfs:subClassOf rdf:resource="#PlantProduct"/>
  </owl:Class>

  <owl:Class rdf:about="#Horticultur">
    <rdfs:subClassOf rdf:resource="#Agricultur"/>
    <rdfs:comment xml:lang="en">Horticulture is the art and science of the cultivation of plants</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Irrigat">
    <rdfs:subClassOf rdf:resource="#SoilCultiv"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&comp;#Water"/>
        <owl:onProperty rdf:resource="&crela;#hasSubst"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#PlantProduct">
    <rdfs:subClassOf rdf:resource="#Agricultur"/>
  </owl:Class>

  <owl:Class rdf:about="#SoilCultiv">
    <rdfs:subClassOf rdf:resource="#Agricultur"/>
  </owl:Class>

<!-- Animal production -->
  <owl:Class rdf:about="#AnimalProduct">
    <rdfs:subClassOf rdf:resource="#Agricultur"/>
  </owl:Class>

  <owl:Class rdf:about="#Apicultur">
    <rdfs:subClassOf rdf:resource="#AnimalProduct"/>
  </owl:Class>

  <owl:Class rdf:about="#Aquacultur">
    <rdfs:subClassOf rdf:resource="#Agricultur"/>
   <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&screla;#hasSpeci"/>
        <owl:allValuesFrom rdf:resource="&anim;#MarineAnim"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Beekeep">
    <owl:equivalentClass rdf:resource="#Apicultur" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&screla;#hasSpeci"/>
        <owl:allValuesFrom rdf:resource="&anim;#Bee"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Breed">
    <rdfs:subClassOf rdf:resource="#AnimalProduct"/>
  </owl:Class>

  <owl:Class rdf:about="#FishProduct">
    <rdfs:subClassOf rdf:resource="#AnimalProduct"/>
    <rdfs:subClassOf rdf:resource="#Aquacultur"/>
  </owl:Class>

  <owl:Class rdf:about="#Fish">
    <rdfs:subClassOf rdf:resource="&comm;#Product"/>
  </owl:Class>

  <owl:Class rdf:about="#Overfish">
    <rdfs:subClassOf rdf:resource="#Fish"/>
  </owl:Class>

  <owl:Class rdf:about="#Sericultur">
    <rdfs:subClassOf rdf:resource="#AnimalProduct"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&screla;#hasSpeci"/>
        <owl:allValuesFrom rdf:resource="&anim;#Silkworm"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ShellfishProduct">
    <rdfs:subClassOf rdf:resource="#AnimalProduct"/>
    <rdfs:subClassOf rdf:resource="#Aquacultur"/>
  </owl:Class>

</rdf:RDF>
