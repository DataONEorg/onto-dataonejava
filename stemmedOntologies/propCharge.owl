<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
  <!ENTITY prop "http://sweet.jpl.nasa.gov/2.3/prop.owl">
  <!ENTITY quan "http://sweet.jpl.nasa.gov/2.3/propQuantity.owl">
  <!ENTITY proc "http://sweet.jpl.nasa.gov/2.3/procPhysical.owl">
  <!ENTITY spac "http://sweet.jpl.nasa.gov/2.3/propSpace.owl">
  <!ENTITY charge "http://sweet.jpl.nasa.gov/2.3/propCharge.owl">
  <!ENTITY ion   "http://sweet.jpl.nasa.gov/2.3/matrIon.owl">
  <!ENTITY units "http://sweet.jpl.nasa.gov/2.3/reprSciUnits.owl">
  <!ENTITY screla "http://sweet.jpl.nasa.gov/2.3/relaSci.owl">
  <!ENTITY crela "http://sweet.jpl.nasa.gov/2.3/relaChemical.owl">
  <!ENTITY rela "http://sweet.jpl.nasa.gov/2.3/rela.owl">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>

<rdf:RDF xml:base="&charge;"
         xmlns:owl="&owl;"
         xmlns:units="&units;"
         xmlns:screla="&screla;"
         xmlns:crela="&crela;"
         xmlns:rela="&rela;"
         xmlns:charge="&charge;"
         xmlns:ion="&ion;"
         xmlns:prop="&prop;"
         xmlns:quan="&quan;"
         xmlns:proc="&proc;"
         xmlns:spac="&spac;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;"
 	 xmlns:xsd="&xsd;">

<!-- Ontology Information -->
<!-- Ontology Information -->
  <owl:Ontology rdf:about="" owl:versionInfo="2.3">
     <rdfs:label>SWEET Ontology</rdfs:label>
     <owl:imports rdf:resource="&units;"/>
     <owl:imports rdf:resource="&ion;"/>
     <owl:imports rdf:resource="&proc;"/>
     <owl:imports rdf:resource="&screla;"/>
     <owl:imports rdf:resource="&crela;"/>
     <owl:imports rdf:resource="&rela;"/>
     <owl:imports rdf:resource="&spac;"/>
     <owl:imports rdf:resource="&prop;"/>
     <owl:imports rdf:resource="&quan;"/>
  </owl:Ontology>

  <owl:Class rdf:about="#Charg">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
  </owl:Class>
 
  <owl:Class rdf:about="#ChargeDistribut">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf rdf:resource="&spac;#SpatialDistribut" />
  </owl:Class>

<!-- Scalars -->
  <owl:Class rdf:about="#ElectricPotenti">
    <rdfs:subClassOf rdf:resource="&quan;#ScalarPotenti"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&screla;#measureOf" />
        <owl:allValuesFrom rdf:resource="#ChargeDistribut" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#volt"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ElectricCurrentDens">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#amperePerMeterSquar"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ElectricFieldStrength">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#voltPerMet"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ElectricCurr">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#amper"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ElectricCharg">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#coulomb"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ElectricFluxDens">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf rdf:resource="&quan;#Flux"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#coulombPerMeterSquar"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ElectricChargeDens">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#coulombPerMeterCub"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

<!-- Vectors -->
  <owl:Class rdf:about="#ElectricDipoleMo">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf rdf:resource="&quan;#VectorQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#coulombMet"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#MagneticMo">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf rdf:resource="&quan;#VectorQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#joulePerTesla"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#DielectricConst">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf rdf:resource="&quan;#PhysicalConst"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#dimensionlessUnit"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#Permitt">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#faradPerMet"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

<!-- Magnetic -->
  <owl:Class rdf:about="#Inductanc">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&rela;#hasProcess" />
        <owl:allValuesFrom rdf:resource="&proc;#Magnet" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#henri"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#MagnetiGradi">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
  </owl:Class>

  <owl:Class rdf:about="#Permeabl">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#henryPerMet"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#MagneticFluxDens">
    <rdfs:subClassOf rdf:resource="&quan;#Flux"/>
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#tesla"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#MagneticFieldStrength">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#amperePerMet"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#MagneticDipoleMo">
    <rdfs:subClassOf rdf:resource="#MagneticMo" />
  </owl:Class>

  <owl:Class rdf:about="#MagneticFlux">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf rdf:resource="&quan;#Flux"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#weber"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#ExchangeCapac">
    <rdfs:subClassOf rdf:resource="&prop;#ReactionProperti"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#perKilogramMol"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#CationExchangeCapac">
    <rdfs:subClassOf rdf:resource="#ExchangeCapac"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&crela;#hasSubst"/>
        <owl:allValuesFrom rdf:resource="&ion;#Cation" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

<!-- Capacity  -->
  <owl:Class rdf:about="#Capacit">
    <rdfs:subClassOf rdf:resource="&quan;#ElectromagneticQuant"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:resource="&units;#farad"/>
        <owl:onProperty rdf:resource="&screla;#hasDefaultUnit"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

</rdf:RDF>
